    cacontology-street:contactDuration 5.0 ;
    cacontology-street:initialContactLocation "Mother Gaston Boulevard, Brooklyn, NY" ;
    uco-action:startTime "2024-04-28T14:00:00Z"^^xsd:dateTime ;
    uco-core:description "Initial contact between Deandre Lee and 15-year-old victim on Mother Gaston Boulevard." .

ex:PhoneChargingPretext rdf:type cacontology-street:PhoneChargingOffer ;
    rdfs:label "Phone Charging Pretext"@en ;
    cacontology-street:pretextUsed "phone_charging" ;
