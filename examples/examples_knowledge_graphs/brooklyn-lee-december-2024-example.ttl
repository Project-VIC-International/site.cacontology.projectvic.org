ex:LocationTransition rdf:type cacontology-street:LocationTransition ;
    rdfs:label "Street to Highway Location Transition"@en ;
    cacontology-street:transportationMethod "personal_vehicle" ;
    cacontology-street:distanceFromContact 2.5 ;  # Approximate distance in miles
    uco-core:description "Transportation of victim from Mother Gaston Boulevard to isolated highway location." .

ex:SecondaryLocationExploitation rdf:type cacontology-street:SecondaryLocationExploitation ;
    rdfs:label "Highway Location Exploitation"@en ;
    cacontology-street:exploitationLocation "Isolated highway location" ;
