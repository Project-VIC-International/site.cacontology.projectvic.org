# =============================================================================
# BROOKLYN DA v. DEANDRE LEE (DECEMBER 2024) - STREET RECRUITMENT CASE EXAMPLE
# =============================================================================

# Case Overview: 29-year-old Deandre Lee sentenced to 10 years for attempted sex 
# trafficking of a 15-year-old girl through street-based recruitment using phone 
# charging pretext, rapid escalation to assault and trafficking proposition

# =============================================================================
# CASE METADATA
# =============================================================================

ex:BrooklynLeeCase rdf:type case-investigation:Investigation ;
    rdfs:label "Brooklyn DA v. Deandre Lee - Attempted Sex Trafficking Case"@en ;
    rdfs:comment "Street-based trafficking recruitment case involving phone charging pretext, rapid escalation, and direct trafficking proposition."@en ;
    case-investigation:investigationStatus "closed" ;
    case-investigation:startDate "2024-04-28"^^xsd:date ;
    case-investigation:endDate "2024-12-18"^^xsd:date ;
    uco-core:description "Investigation and prosecution of Deandre Lee for attempted sex trafficking of a 15-year-old girl through street-based recruitment on Mother Gaston Boulevard in Brooklyn." .

# =============================================================================
# DEFENDANT
# =============================================================================

ex:DeandreeLee rdf:type uco-identity:Person ;
    rdfs:label "Deandre Lee"@en ;
    uco-core:name "Deandre Lee" ;
    uco-identity:birthDate "1995-01-01"^^xsd:date ;  # Approximate based on age 29 in 2024
    uco-core:description "29-year-old defendant from East New York, Brooklyn, convicted of attempted sex trafficking of a child." .

ex:DeandreeLeeRole rdf:type cacontology-trafficking:Trafficker ;
    rdfs:label "Deandre Lee - Trafficker Role"@en ;
    uco-core:roleOf ex:DeandreeLee ;
    uco-core:description "Role as trafficker attempting to recruit 15-year-old victim for commercial sexual exploitation." .

# =============================================================================
# VICTIM
# =============================================================================

ex:Victim15YearOld rdf:type uco-identity:Person ;
    rdfs:label "15-Year-Old Victim"@en ;
    uco-core:name "Victim (15 years old)" ;
    uco-identity:birthDate "2009-01-01"^^xsd:date ;  # Approximate based on age 15 in 2024
    uco-core:description "15-year-old female victim of attempted sex trafficking." .

ex:VictimRole rdf:type cacontology-trafficking:TraffickingVictim ;
    rdfs:label "15-Year-Old Victim Role"@en ;
    uco-core:roleOf ex:Victim15YearOld ;
    uco-core:description "Role as victim of attempted sex trafficking and sexual assault." .

# =============================================================================
# LOCATIONS
# =============================================================================

ex:MotherGastonBoulevard rdf:type cacontology-street:StreetRecruitmentLocation ;
    rdfs:label "Mother Gaston Boulevard"@en ;
    uco-location:address "Mother Gaston Boulevard, Brooklyn, NY" ;
    uco-core:description "Street location in Brooklyn where initial contact occurred between defendant and victim." .

ex:MotherGastonBoulevardArea rdf:type cacontology-street:ResidentialArea ;
    rdfs:label "Mother Gaston Boulevard Area"@en ;
    uco-core:description "Residential area in Brooklyn where victim was walking when approached." .

ex:VehicleLocation rdf:type cacontology-street:VehicleLocation ;
    rdfs:label "Defendant's Vehicle"@en ;
    uco-core:description "Vehicle used to isolate victim and transport to secondary location for exploitation." .

ex:HighwayLocation rdf:type cacontology-street:HighwayLocation ;
    rdfs:label "Highway Location"@en ;
    uco-core:description "Isolated location near highway where sexual assault and trafficking proposition occurred." .

# =============================================================================
# VULNERABILITY INDICATORS
# =============================================================================

ex:VictimAgeVulnerability rdf:type cacontology-street:AgeVulnerabilityIndicator ;
    rdfs:label "Victim Age Vulnerability"@en ;
    cacontology-street:apparentAge 15 ;
    uco-core:description "15-year-old victim's youth making her vulnerable to exploitation." .

ex:VictimIsolationVulnerability rdf:type cacontology-street:IsolationVulnerabilityIndicator ;
    rdfs:label "Victim Isolation Vulnerability"@en ;
    cacontology-street:isolationLevel "alone" ;
