# =============================================================================
# Buffalo/Vermont Interstate Transportation Case Example
# Case: Ian Blochwitz, charged with transporting minor across state lines
# Demonstrates enhanced ontology modules for anonymous platform contact,
# online-to-offline progression, and interstate transportation framework
# =============================================================================

# Case Overview
<https://example.org/cases/buffalo-vermont-transport> rdf:type cacontology-multi:InterstateTransportationOffense ;
    rdfs:label "Buffalo/Vermont Interstate Transportation Case"@en ;
    cacontology-multi:transportationIntent "sexual_activity" ;
    cacontology-multi:statesCrossed 2 ;
    cacontology-multi:crossingDocumentation "witness_testimony" ;
    cacontology-multi:jurisdictionTriggerType "interstate_transport" ;
    uco-core:startTime "2024-11-01T10:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-11-15T18:00:00Z"^^xsd:dateTime .

# Perpetrator
<https://example.org/persons/ian-blochwitz> rdf:type cacontology-grooming:OnlinePredator ;
    rdfs:label "Ian Blochwitz"@en ;
    uco-identity:hasName "Ian Blochwitz" ;
    uco-location:hasLocation <https://example.org/locations/buffalo-ny> .

# Victim (anonymized)
<https://example.org/persons/victim-minor> rdf:type cacontology-grooming:ChildVictim ;
    rdfs:label "Minor Victim"@en ;
    uco-location:hasLocation <https://example.org/locations/vermont> .

# Locations
<https://example.org/locations/buffalo-ny> rdf:type uco-location:Location ;
    rdfs:label "Buffalo, New York"@en ;
    uco-location:hasLocationName "Buffalo, NY" .

<https://example.org/locations/vermont> rdf:type uco-location:Location ;
    rdfs:label "Vermont"@en ;
    uco-location:hasLocationName "Vermont" .

# =============================================================================
# ANONYMOUS PLATFORM CONTACT MODELING
# =============================================================================

# Anonymous Chat Platform Used for Initial Contact
<https://example.org/platforms/anonymous-chat-app> rdf:type cacontology-platforms:AnonymousChatPlatform ;
    rdfs:label "Anonymous Chat Application"@en ;
    cacontology-platforms:allowsAnonymousChat true ;
    cacontology-platforms:guestAccountsAllowed true ;
    cacontology-platforms:ageVerificationRequired false ;
    cacontology-platforms:anonymousUserDataRetention 30 ;
    cacontology-platforms:conversationLogging true .

# Anonymous Contact Features
<https://example.org/features/guest-chat> rdf:type cacontology-platforms:GuestChatFeature ;
    rdfs:label "Guest Chat Feature"@en .

<https://example.org/features/random-matching> rdf:type cacontology-platforms:RandomUserMatching ;
    rdfs:label "Random User Matching"@en ;
    cacontology-platforms:contactInitiationMethod "random_matching" ;
    cacontology-platforms:moderationPresence "minimal" .

# Anonymity Protection Level
<https://example.org/protection/full-anonymous> rdf:type cacontology-platforms:AnonymityProtectionLevel ;
    rdfs:label "Full Anonymous Protection"@en ;
    cacontology-platforms:anonymityLevel "full_anonymous" ;
    cacontology-platforms:crossPlatformAnonymity false .

# Initial Anonymous Contact
<https://example.org/contacts/initial-anonymous> rdf:type cacontology-grooming:AnonymousPlatformContact ;
    rdfs:label "Initial Anonymous Contact"@en ;
    cacontology-grooming:platformAnonymityLevel "full_anonymous" ;
    cacontology-grooming:identityVerificationRequired false ;
    cacontology-grooming:anonymityMaintainedDuration 7 ;
    cacontology-grooming:targetsVictim <https://example.org/persons/victim-minor> ;
    cacontology-grooming:primaryPlatform <https://example.org/platforms/anonymous-chat-app> .

# Anonymous Initiation Behavior
<https://example.org/behaviors/anonymous-initiation> rdf:type cacontology-grooming:AnonymousInitiation ;
    rdfs:label "Anonymous Contact Initiation"@en ;
    cacontology-grooming:anonymousContactMethod "random_matching" ;
    cacontology-grooming:initialContactPretense "casual_conversation" ;
    uco-core:startTime "2024-11-01T10:00:00Z"^^xsd:dateTime .

# Trust Building Through Anonymity
<https://example.org/behaviors/trust-through-anonymity> rdf:type cacontology-grooming:TrustThroughAnonymity ;
    rdfs:label "Building Trust Through Anonymous Contact"@en ;
    cacontology-grooming:groomingStage "trust_building" ;
    cacontology-grooming:behaviorDuration 7 ;
    cacontology-grooming:usesGifts false ;
    cacontology-grooming:requestsSecrecy true .

# =============================================================================
# ONLINE-TO-OFFLINE PROGRESSION MODELING
# =============================================================================

# Online to Offline Escalation Pattern
<https://example.org/patterns/online-to-offline> rdf:type cacontology-grooming:OnlineToOfflineProgression ;
    rdfs:label "Online to Offline Progression"@en ;
    cacontology-grooming:progressionDuration 14 ;
    cacontology-grooming:patternConfidence 0.95 ;
    cacontology-grooming:escalationRate 14 .

# Physical Meeting Arrangement
<https://example.org/arrangements/physical-meeting> rdf:type cacontology-grooming:PhysicalMeetingArrangement ;
    rdfs:label "Vermont Meeting Arrangement"@en ;
    cacontology-grooming:meetingLocationSpecified true ;
    uco-core:startTime "2024-11-10T12:00:00Z"^^xsd:dateTime .

# Travel Arrangement
<https://example.org/arrangements/travel-plan> rdf:type cacontology-grooming:TravelArrangement ;
    rdfs:label "Interstate Travel Arrangement"@en ;
    cacontology-grooming:transportationProposed "predator_travel" .

# Interstate Transport Plan
<https://example.org/plans/interstate-transport> rdf:type cacontology-grooming:InterstateTransportPlan ;
    rdfs:label "Buffalo to Vermont Transport Plan"@en ;
    cacontology-grooming:crossesStateBoundaries true .

# Victim Compliance in Physical Meeting
<https://example.org/responses/victim-compliance> rdf:type cacontology-grooming:VictimComplianceInPhysicalMeeting ;
    rdfs:label "Victim Compliance with Meeting"@en ;
    cacontology-grooming:victimTravelCompliance "agreed" .

# Location-Specific Grooming
<https://example.org/behaviors/location-grooming> rdf:type cacontology-grooming:LocationSpecificGrooming ;
    rdfs:label "Vermont Location Grooming"@en ;
    cacontology-grooming:locationControlStrategy "familiarity" .

# Offline Control Establishment
<https://example.org/patterns/offline-control> rdf:type cacontology-grooming:OfflineControlEstablishment ;
    rdfs:label "Offline Control Establishment"@en ;
    cacontology-grooming:offlineControlTactic "emotional_manipulation" .

# =============================================================================
# INTERSTATE TRANSPORTATION FRAMEWORK MODELING
# =============================================================================

# State Line Crossing Event
<https://example.org/events/state-line-crossing> rdf:type cacontology-multi:InterstateVictimTransport ;
    rdfs:label "New York to Vermont State Line Crossing"@en ;
    cacontology-multi:transportationMethod "vehicle" ;
    cacontology-multi:statesCrossed 2 ;
    cacontology-multi:crossingDocumentation "witness_testimony" ;
    cacontology-multi:originatingState <https://example.org/locations/buffalo-ny> ;
    cacontology-multi:destinationState <https://example.org/locations/vermont> ;
    uco-core:startTime "2024-11-15T08:00:00Z"^^xsd:dateTime .

# Federal Jurisdiction Trigger
<https://example.org/triggers/federal-jurisdiction> rdf:type cacontology-multi:FederalJurisdictionTrigger ;
    rdfs:label "Federal Jurisdiction Trigger"@en ;
    cacontology-multi:jurisdictionTriggerType "interstate_transport" .

# Jurisdictional Handoff
<https://example.org/handoffs/state-to-federal> rdf:type cacontology-multi:JurisdictionalHandoff ;
    rdfs:label "State to Federal Handoff"@en ;
    cacontology-multi:handoffType "state_to_federal" ;
    uco-core:startTime "2024-11-16T09:00:00Z"^^xsd:dateTime .

# Interstate Coordination
<https://example.org/coordination/ny-vt-federal> rdf:type cacontology-multi:InterstateCoordination ;
    rdfs:label "NY-VT-Federal Coordination"@en ;
    cacontology-multi:coordinationComplexity "moderate" ;
    cacontology-multi:crossStateCommunication "direct_contact" .

# =============================================================================
# INVESTIGATION AND EVIDENCE
# =============================================================================

# Multi-Jurisdictional Investigation
<https://example.org/investigations/multi-jurisdictional> rdf:type cacontology-multi:MultiJurisdictionalInvestigation ;
    rdfs:label "Buffalo/Vermont Multi-Jurisdictional Investigation"@en ;
    cacontology-multi:investigationScope "interstate" ;
    cacontology-multi:jurisdictionCount 3 ;
    cacontology-multi:agencyCount 4 ;
    cacontology-multi:crossesBorders true ;
    cacontology-multi:coordinationMechanism "joint_operation" ;
    cacontology-multi:legalComplexity "moderate" .

# Evidence Collection
<https://example.org/evidence/chat-logs> rdf:type cacontology-platforms:SocialMediaEvidence ;
    rdfs:label "Anonymous Chat Logs"@en ;
    cacontology-platforms:contentType "text" ;
    cacontology-platforms:evidenceCategory "communication" ;
    cacontology-platforms:socialMediaPlatform "Anonymous Chat App" ;
    cacontology-platforms:foundOnSocialMedia true ;
    cacontology-platforms:searchWarrantRequired true ;
    cacontology-platforms:legalProcessUsed "warrant" ;
    cacontology-platforms:evidenceTimestamp "2024-11-01T10:00:00Z"^^xsd:dateTime ;
    cacontology-platforms:discoveryTimestamp "2024-11-20T14:00:00Z"^^xsd:dateTime .

# Device Evidence
<https://example.org/evidence/mobile-device> rdf:type cacontology-platforms:ElectronicDeviceEvidence ;
    rdfs:label "Perpetrator Mobile Device"@en ;
    cacontology-platforms:deviceType "phone" ;
    cacontology-platforms:foundOnDevice true .

# =============================================================================
# LEGAL PROCEEDINGS AND SENTENCING
# =============================================================================

# Federal Charges
<https://example.org/charges/interstate-transport> rdf:type cacontology-sentencing:FederalCharge ;
    rdfs:label "Interstate Transportation of Minor"@en ;
    cacontology-sentencing:chargeCode "18_USC_2423_a" ;
    cacontology-sentencing:chargeDescription "Transportation of minor for illegal sexual activity" ;
    cacontology-sentencing:jurisdictionLevel "federal" ;
    cacontology-sentencing:maximumSentence 120 ;
    cacontology-sentencing:minimumSentence 120 .

# Transportation-Specific Sentencing
<https://example.org/sentencing/transportation-offense> rdf:type cacontology-sentencing:TransportationOffenseSentencing ;
    rdfs:label "Transportation Offense Sentencing"@en ;
    cacontology-sentencing:transportationEnhancement true ;
    cacontology-sentencing:crossedStateBoundaries true ;
    cacontology-sentencing:victimTransported true .

