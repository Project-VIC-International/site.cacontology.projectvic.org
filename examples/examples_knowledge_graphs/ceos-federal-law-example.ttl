    rdfs:label "Minor Victim 3"@en .

# =============================================================================
# CRIMINAL ACTIONS AND VIOLATIONS
# =============================================================================

example:ProductionAction rdf:type uco-action:Action ;
    rdfs:label "Child Pornography Production Action"@en .

example:DistributionAction rdf:type uco-action:Action ;
    rdfs:label "Child Pornography Distribution Action"@en .

example:SexTraffickingAction rdf:type uco-action:Action ;
    rdfs:label "Sex Trafficking Action"@en .

example:SexTourismAction rdf:type uco-action:Action ;
    rdfs:label "Sex Tourism Action"@en .

# =============================================================================
# FEDERAL LAW RELATIONSHIPS
# =============================================================================

# CEOS enforcement relationships
example:DOJ_CEOS cacontology-federal:enforces example:FederalChildPornographyCase .
example:DOJ_CEOS cacontology-federal:enforces example:FederalSexTraffickingCase .
example:DOJ_CEOS cacontology-federal:enforces example:ObscenityDistributionCase .

# CEOS prosecution relationships
example:CEOSprosecutionExample cacontology-federal:prosecutesUnder example:ProductionCharge .
example:CEOSprosecutionExample cacontology-federal:prosecutesUnder example:SexTraffickingCharge .
example:CEOSprosecutionExample cacontology-federal:prosecutesUnder example:SexTourismCase .

# Criminal action violations
example:ProductionAction cacontology-federal:violates example:ProductionCharge .
example:DistributionAction cacontology-federal:violates example:DistributionCharge .
example:SexTraffickingAction cacontology-federal:violates example:SexTraffickingCharge .
example:SexTourismAction cacontology-federal:violates example:SexTourismCase .

# Defendant charges
example:FederalDefendant cacontology-federal:chargedUnder example:ProductionCharge .
example:FederalDefendant cacontology-federal:chargedUnder example:DistributionCharge .
example:FederalDefendant cacontology-federal:chargedUnder example:PossessionCharge .
example:FederalDefendant cacontology-federal:chargedUnder example:SexTraffickingCharge .

# Child support intersection
example:ChildSupportEvasionCase cacontology-federal:linkedToExploitation example:SexTraffickingCharge .
example:FinancialControlConnection cacontology-federal:enablesControl example:SexTraffickingCharge .

# Extraterritorial relationships
example:SexTourismCase cacontology-federal:occurredIn example:PhilippinesLocation .
example:SexTourismCase cacontology-federal:involvesTravelTo example:PhilippinesLocation .
example:ExtraterritorialProductionCase cacontology-federal:occurredIn example:ThailandLocation .

# Sex offender registration requirements
example:ProductionCharge cacontology-federal:requiresRegistration example:SORNAcomplianceCase .
example:SexTraffickingCharge cacontology-federal:requiresRegistration example:SORNAcomplianceCase .

# Federal jurisdiction establishment
example:InterstateJurisdictionBasis cacontology-federal:establishesJurisdiction example:CEOSprosecutionExample .
example:ForeignCommerceJurisdictionBasis cacontology-federal:enablesFederalProsecution example:CEOSprosecutionExample .

# Multi-charge relationships
example:ProductionCharge cacontology-federal:accompaniedBy example:DistributionCharge .
example:ProductionCharge cacontology-federal:accompaniedBy example:PossessionCharge .
example:SexTraffickingCharge cacontology-federal:enhancedBy example:ProductionCharge . 