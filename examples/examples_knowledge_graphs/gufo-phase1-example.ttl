@prefix example: <http://ontology.icac.org/examples/gufo-phase1#> .
@prefix cacontology-gufo: <https://cacontology.projectvic.org/gufo#> .
@prefix gufo: <http://purl.org/nemo/gufo#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =============================================================================
# PHASE 1 gUFO INTEGRATION EXAMPLE
# =============================================================================

# Investigation Instance (Kind with Phase modeling)
example:Investigation001 rdf:type cacontology-gufo:Investigation ;
    uco-core:name "ICAC Investigation 2025-001" ;
    uco-core:description "Multi-phase investigation beginning with NCMEC cybertip" ;
    rdfs:label "ICAC Investigation 001"@en .

# =============================================================================
# INVESTIGATION PHASES WITH gUFO TEMPORAL FRAMEWORK
# =============================================================================

# Initial Phase Instance
example:InitialPhase001 rdf:type cacontology-gufo:InitialPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:hasPhaseEndPoint "2025-01-01T10:30:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:phaseDuration "PT2H30M"^^xsd:duration ;
    rdfs:label "Initial Investigation Phase 001"@en .

# Analysis Phase Instance
example:AnalysisPhase001 rdf:type cacontology-gufo:AnalysisPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-01T10:30:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:hasPhaseEndPoint "2025-01-03T16:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:phaseDuration "P2DT5H30M"^^xsd:duration ;
    rdfs:label "Analysis Investigation Phase 001"@en .

# Legal Process Phase Instance
example:LegalProcessPhase001 rdf:type cacontology-gufo:LegalProcessPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-03T16:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:hasPhaseEndPoint "2025-01-10T14:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:phaseDuration "P7DT22H"^^xsd:duration ;
    rdfs:label "Legal Process Investigation Phase 001"@en .

# Evidence Phase Instance
example:EvidencePhase001 rdf:type cacontology-gufo:EvidencePhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-10T14:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:hasPhaseEndPoint "2025-01-25T17:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-gufo:phaseDuration "P15DT3H"^^xsd:duration ;
    rdfs:label "Evidence Investigation Phase 001"@en .

# Current Phase (Resolution)
example:ResolutionPhase001 rdf:type cacontology-gufo:ResolutionPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-25T17:00:00Z"^^xsd:dateTimeStamp ;
    rdfs:label "Resolution Investigation Phase 001"@en .

# =============================================================================
# PERSON INSTANCES WITH gUFO ROLE MODELING
# =============================================================================

# Base Persons (Kind instances)
example:Detective_Smith rdf:type cacontology-gufo:Person ;
    uco-core:name "Detective John Smith" ;
    rdfs:label "Detective John Smith"@en .

example:Analyst_Jones rdf:type cacontology-gufo:Person ;
    uco-core:name "Analyst Sarah Jones" ;
    rdfs:label "Analyst Sarah Jones"@en .

example:Victim_Child rdf:type cacontology-gufo:Person ;
    uco-core:name "Child Victim (pseudonym: Emma)" ;
    rdfs:label "Child Victim Emma"@en .

example:Suspect_Williams rdf:type cacontology-gufo:Person ;
    uco-core:name "Suspect Robert Williams" ;
    rdfs:label "Suspect Robert Williams"@en .

example:Witness_Parent rdf:type cacontology-gufo:Person ;
    uco-core:name "Parent Witness Mary Johnson" ;
    rdfs:label "Parent Witness Mary Johnson"@en .

# =============================================================================
# ROLE INSTANCES WITH TEMPORAL BOUNDARIES
# =============================================================================

# Detective in Investigator Role
example:InvestigatorRole001 rdf:type cacontology-gufo:InvestigatorRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Detective Smith as Lead Investigator" ;
    rdfs:label "Investigator Role 001"@en .

# Analyst in Secondary Investigator Role
example:InvestigatorRole002 rdf:type cacontology-gufo:InvestigatorRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-01T10:30:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Analyst Jones as Secondary Investigator" ;
    rdfs:label "Investigator Role 002"@en .

# Child in Victim Role
example:VictimRole001 rdf:type cacontology-gufo:VictimRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Child Emma as Primary Victim" ;
    rdfs:label "Victim Role 001"@en .

# Suspect in Offender Role
example:OffenderRole001 rdf:type cacontology-gufo:OffenderRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-03T16:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Robert Williams as Suspected Offender" ;
    rdfs:label "Offender Role 001"@en .

# Parent in Witness Role
example:WitnessRole001 rdf:type cacontology-gufo:WitnessRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-02T14:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Mary Johnson as Parent Witness" ;
    rdfs:label "Witness Role 001"@en .

# Parent also becomes Informant (multiple roles)
example:InformantRole001 rdf:type cacontology-gufo:InformantRole ;
    cacontology-gufo:hasRoleBeginPoint "2025-01-15T11:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Mary Johnson as Informant providing additional intelligence" ;
    rdfs:label "Informant Role 001"@en .

# =============================================================================
# INVESTIGATION EVENTS (Concrete Occurrences)
# =============================================================================

# Cybertip Receipt Event
example:ReceiveCybertipEvent001 rdf:type cacontology-gufo:ReceiveCybertipEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-01T08:15:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "NCMEC Cybertip 12345 Receipt" ;
    rdfs:label "Receive Cybertip Event 001"@en .

# Cybertip Review Event
example:ReviewCybertipEvent001 rdf:type cacontology-gufo:ReviewCybertipEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T10:30:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-01T16:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Cybertip Analysis and Initial Assessment" ;
    rdfs:label "Review Cybertip Event 001"@en .

# Search Warrant Event
example:LegalProcessEvent001 rdf:type cacontology-gufo:LegalProcessEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-03T16:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-03T18:30:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Search Warrant Execution at Suspect Residence" ;
    rdfs:label "Legal Process Event 001"@en .

# Digital Evidence Review Event
example:EvidenceReviewEvent001 rdf:type cacontology-gufo:EvidenceReviewEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-10T14:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-25T17:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Digital Forensics Analysis of Seized Devices" ;
    rdfs:label "Evidence Review Event 001"@en .

# Victim Rescue Event (future/planned)
example:VictimRescueEvent001 rdf:type cacontology-gufo:VictimRescueEvent ;
    uco-core:name "Planned Victim Support and Safety Measures" ;
    rdfs:label "Victim Rescue Event 001"@en .

# =============================================================================
# INVESTIGATION LIFECYCLE SITUATIONS (States that Hold)
# =============================================================================

# Investigation Pending Situation (initial state)
example:PendingSituation001 rdf:type cacontology-gufo:InvestigationPendingSituation ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-01T08:15:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Investigation pending analyst assignment" ;
    rdfs:label "Investigation Pending Situation 001"@en .

# Investigation Active Situation (current state)
example:ActiveSituation001 rdf:type cacontology-gufo:InvestigationActiveSituation ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T08:15:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Investigation actively progressing through phases" ;
    rdfs:label "Investigation Active Situation 001"@en .

# =============================================================================
# CRIMINAL EVENTS WITH gUFO MODELING
# =============================================================================

# Base Criminal Event
example:CSAMincident001 rdf:type cacontology-gufo:CSAMIncident ;
    gufo:hasBeginPointInXSDDateTimeStamp "2024-12-15T00:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2024-12-20T00:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "CSAM Distribution Incident via Social Media Platform" ;
    rdfs:label "CSAM Incident 001"@en .

# Grooming Event (related criminal activity)
example:GroomingEvent001 rdf:type cacontology-gufo:GroomingSolicitation ;
    gufo:hasBeginPointInXSDDateTimeStamp "2024-11-01T00:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2024-12-15T00:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Online Grooming Leading to CSAM Production" ;
    rdfs:label "Grooming Event 001"@en .

# =============================================================================
# PHASE RELATIONSHIPS AND TRANSITIONS
# =============================================================================

# Investigation to Phase Relationships
example:Investigation001 cacontology-gufo:inPhase example:ResolutionPhase001 .
example:Investigation001 cacontology-gufo:hasPhase example:InitialPhase001 .
example:Investigation001 cacontology-gufo:hasPhase example:AnalysisPhase001 .
example:Investigation001 cacontology-gufo:hasPhase example:LegalProcessPhase001 .
example:Investigation001 cacontology-gufo:hasPhase example:EvidencePhase001 .
example:Investigation001 cacontology-gufo:hasPhase example:ResolutionPhase001 .

# Phase Precedence Chain
example:InitialPhase001 cacontology-gufo:precedesPhase example:AnalysisPhase001 .
example:AnalysisPhase001 cacontology-gufo:precedesPhase example:LegalProcessPhase001 .
example:LegalProcessPhase001 cacontology-gufo:precedesPhase example:EvidencePhase001 .
example:EvidencePhase001 cacontology-gufo:precedesPhase example:ResolutionPhase001 .

# =============================================================================
# ROLE RELATIONSHIPS AND PARTICIPATION
# =============================================================================

# Person to Role Relationships
example:Detective_Smith cacontology-gufo:playsRole example:InvestigatorRole001 .
example:Analyst_Jones cacontology-gufo:playsRole example:InvestigatorRole002 .
example:Victim_Child cacontology-gufo:playsRole example:VictimRole001 .
example:Suspect_Williams cacontology-gufo:playsRole example:OffenderRole001 .
example:Witness_Parent cacontology-gufo:playsRole example:WitnessRole001 .
example:Witness_Parent cacontology-gufo:playsRole example:InformantRole001 .

# Investigation to Role Relationships
example:Investigation001 cacontology-gufo:hasRole example:InvestigatorRole001 .
example:Investigation001 cacontology-gufo:hasRole example:InvestigatorRole002 .
example:Investigation001 cacontology-gufo:hasRole example:VictimRole001 .
example:Investigation001 cacontology-gufo:hasRole example:OffenderRole001 .
example:Investigation001 cacontology-gufo:hasRole example:WitnessRole001 .
example:Investigation001 cacontology-gufo:hasRole example:InformantRole001 .

# Role Participation in Investigation
example:InvestigatorRole001 cacontology-gufo:participatesInInvestigation example:Investigation001 .
example:InvestigatorRole002 cacontology-gufo:participatesInInvestigation example:Investigation001 .
example:VictimRole001 cacontology-gufo:participatesInInvestigation example:Investigation001 .
example:OffenderRole001 cacontology-gufo:participatesInInvestigation example:Investigation001 .
example:WitnessRole001 cacontology-gufo:participatesInInvestigation example:Investigation001 .
example:InformantRole001 cacontology-gufo:participatesInInvestigation example:Investigation001 .

