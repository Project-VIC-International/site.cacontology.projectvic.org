    cacontology-temporal:hasMaximumDuration "PT8H"^^xsd:duration ;
    cacontology-temporal:hasTypicalDuration "PT3H"^^xsd:duration ;
    cacontology-temporal:urgencyLevel "4"^^xsd:integer ;
    cacontology-temporal:phaseEfficiency "1.33"^^xsd:decimal ;  # 4h actual / 3h typical
    cacontology-temporal:phaseCompletionRate "1.0"^^xsd:decimal ;
    rdfs:label "Initial Phase 001"@en .

# Analysis Phase (extended due to complexity)
example:AnalysisPhase001 rdf:type cacontology-gufo:AnalysisPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-01T12:00:00Z"^^xsd:dateTimeStamp ;
