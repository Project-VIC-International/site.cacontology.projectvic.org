    cacontology-gufo:phaseDuration "P4DT4H"^^xsd:duration ;
    cacontology-temporal:hasMinimumDuration "P1D"^^xsd:duration ;
    cacontology-temporal:hasMaximumDuration "P7D"^^xsd:duration ;
    cacontology-temporal:hasTypicalDuration "P3D"^^xsd:duration ;
