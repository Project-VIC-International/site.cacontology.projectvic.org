# Evidence Phase (with suspension/resumption)
example:EvidencePhase001 rdf:type cacontology-gufo:EvidencePhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-01-15T14:00:00Z"^^xsd:dateTimeStamp ;
