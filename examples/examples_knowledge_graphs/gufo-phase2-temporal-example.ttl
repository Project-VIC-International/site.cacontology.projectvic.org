# =============================================================================
# PHASE 2 gUFO TEMPORAL ENHANCEMENT EXAMPLE
# Complex Investigation Lifecycle with Suspension/Resumption and Multi-Jurisdiction Coordination
# =============================================================================

# =============================================================================
# INVESTIGATION LIFECYCLE INSTANCE
# =============================================================================

# Investigation Lifecycle
example:Lifecycle001 rdf:type cacontology-temporal:InvestigationLifecycle ;
    uco-core:name "Complex Multi-Jurisdiction Investigation Lifecycle" ;
    rdfs:label "Investigation Lifecycle 001"@en ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp .

# Main Investigation
example:Investigation001 rdf:type cacontology-gufo:Investigation ;
    uco-core:name "ICAC Investigation 2025-001 - Multi-Jurisdiction CSAM Distribution" ;
    uco-core:description "Complex investigation involving CSAM distribution across multiple states with international coordination" ;
    gufo:participatesIn example:Lifecycle001 ;
    cacontology-temporal:StatuteOfLimitationsDeadline "2030-01-01T23:59:59Z"^^xsd:dateTimeStamp ;
    rdfs:label "Multi-Jurisdiction Investigation 001"@en .

# =============================================================================
# INVESTIGATION PHASES WITH TEMPORAL CONSTRAINTS
