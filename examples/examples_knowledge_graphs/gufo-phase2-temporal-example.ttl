# Resolution Phase (current)
example:ResolutionPhase001 rdf:type cacontology-gufo:ResolutionPhase ;
    cacontology-gufo:hasPhaseBeginPoint "2025-02-20T17:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:urgencyLevel "3"^^xsd:integer ;
    rdfs:label "Resolution Phase 001"@en .

# =============================================================================
# PHASE TRANSITION EVENTS WITH TEMPORAL MODELING
# =============================================================================

# Investigation Initiation
example:InitiationEvent001 rdf:type cacontology-temporal:InitiationEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-01T08:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:transitionsTo example:InitialPhase001 ;
    uco-core:name "Investigation initiation upon NCMEC cybertip receipt" ;
    rdfs:label "Initiation Event 001"@en .

# Transition to Analysis
example:AnalysisTransition001 rdf:type cacontology-temporal:AnalysisTransitionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-01T12:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-01T12:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:transitionsFrom example:InitialPhase001 ;
    cacontology-temporal:transitionsTo example:AnalysisPhase001 ;
    uco-core:name "Transition to analysis phase after initial assessment completion" ;
    rdfs:label "Analysis Transition 001"@en .

# Transition to Legal Process
example:LegalTransition001 rdf:type cacontology-temporal:LegalProcessTransitionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-05T16:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-05T16:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:transitionsFrom example:AnalysisPhase001 ;
    cacontology-temporal:transitionsTo example:LegalProcessPhase001 ;
    uco-core:name "Transition to legal process phase upon identification of sufficient probable cause" ;
    rdfs:label "Legal Process Transition 001"@en .

# Transition to Evidence
example:EvidenceTransition001 rdf:type cacontology-temporal:EvidenceTransitionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-15T14:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-15T14:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:transitionsFrom example:LegalProcessPhase001 ;
    cacontology-temporal:transitionsTo example:EvidencePhase001 ;
    uco-core:name "Transition to evidence phase after search warrant execution" ;
    rdfs:label "Evidence Transition 001"@en .

# Transition to Resolution
example:ResolutionTransition001 rdf:type cacontology-temporal:ResolutionTransitionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-02-20T17:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-02-20T17:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:transitionsFrom example:EvidencePhase001 ;
    cacontology-temporal:transitionsTo example:ResolutionPhase001 ;
    uco-core:name "Transition to resolution phase after evidence analysis completion" ;
    rdfs:label "Resolution Transition 001"@en .

# =============================================================================
# SUSPENSION AND RESUMPTION TEMPORAL PATTERNS
# =============================================================================

# Investigation Suspension (during Evidence Phase)
example:SuspensionEvent001 rdf:type cacontology-temporal:SuspensionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-25T10:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-01-25T10:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:suspends example:Investigation001 ;
    cacontology-temporal:createsSuspension example:SuspensionSituation001 ;
    uco-core:name "Investigation suspended pending federal task force coordination" ;
    rdfs:label "Suspension Event 001"@en .

# Suspended Situation
example:SuspensionSituation001 rdf:type cacontology-temporal:SuspendedSituation ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-01-25T10:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-02-05T14:00:00Z"^^xsd:dateTimeStamp ;
    uco-core:name "Investigation suspended for federal coordination and resource allocation" ;
    rdfs:label "Suspension Situation 001"@en .

# Investigation Resumption
example:ResumptionEvent001 rdf:type cacontology-temporal:ResumptionEvent ;
    gufo:hasBeginPointInXSDDateTimeStamp "2025-02-05T14:00:00Z"^^xsd:dateTimeStamp ;
    gufo:hasEndPointInXSDDateTimeStamp "2025-02-05T14:00:00Z"^^xsd:dateTimeStamp ;
    cacontology-temporal:resumes example:Investigation001 ;
    cacontology-temporal:terminatesSuspension example:SuspensionSituation001 ;
    uco-core:name "Investigation resumed with federal task force support" ;
    rdfs:label "Resumption Event 001"@en .

