# =============================================================================
# HAITIAN ORPHANAGE CASE EXAMPLE - MICHAEL KARL GEILENFELD
# US v. Michael Karl Geilenfeld (S.D. Florida, May 2025)
# 210-year sentence for operating orphanage as exploitation operation
# =============================================================================

<https://cacontology.projectvic.org/examples/haitian-orphanage-geilenfeld>
    rdf:type owl:Ontology ;
    rdfs:label "Haitian Orphanage Geilenfeld Case Example"@en ;
    rdfs:comment "Example modeling the Michael Karl Geilenfeld case involving systematic exploitation at St. Joseph's Home for Boys in Haiti over more than two decades."@en ;
    owl:versionInfo "1.0.0" .

# Namespace for this example
@prefix geilenfeld-case: <https://cacontology.projectvic.org/examples/haitian-orphanage-geilenfeld#> .

# =============================================================================
# PERPETRATOR AND INSTITUTIONAL LEADERSHIP
# =============================================================================

geilenfeld-case:michael-karl-geilenfeld rdf:type uco-identity:Person ;
    rdfs:label "Michael Karl Geilenfeld"@en ;
    rdfs:comment "73-year-old founder and director of St. Joseph's Home for Boys in Haiti, convicted of systematic child exploitation."@en ;
    uco-core:name "Michael Karl Geilenfeld" ;
    uco-identity:age 73 ;
    cacontology-institutional:holdsRole geilenfeld-case:founder-director-role .

geilenfeld-case:founder-director-role rdf:type cacontology-institutional:OrphanageDirector ,
                                              cacontology-institutional:InstitutionalFounder ,
                                              cacontology-institutional:TrustedAdultRole ;
    rdfs:label "Orphanage Founder and Director Role"@en ;
    rdfs:comment "Role as founder and long-term director of St. Joseph's Home for Boys with complete institutional authority."@en ;
    cacontology-institutional:authorityLevel "founder" ;
    cacontology-institutional:trustLevelExploited "complete" .

# =============================================================================
# CHARITABLE ORGANIZATION AND INSTITUTIONAL STRUCTURE
# =============================================================================

geilenfeld-case:st-josephs-home-for-boys rdf:type cacontology-institutional:Orphanage ,
                                                  cacontology-institutional:CharitableOrganization ;
    rdfs:label "St. Joseph's Home for Boys"@en ;
    rdfs:comment "Orphanage founded in Haiti in 1985 as a home for orphaned, impoverished, and vulnerable children."@en ;
    uco-core:name "St. Joseph's Home for Boys" ;
    cacontology-institutional:foundingYear "1985"^^xsd:gYear ;
    cacontology-institutional:operationDurationYears 25 ; # 1985-2010+
    cacontology-institutional:childrenServed 100 ; # Estimated number over 25 years
    cacontology-institutional:foundedBy geilenfeld-case:michael-karl-geilenfeld ;
    cacontology-institutional:maintainedBy geilenfeld-case:michael-karl-geilenfeld ;
    cacontology-institutional:operatesInCountry geilenfeld-case:haiti-location ;
    cacontology-institutional:servesPopulation geilenfeld-case:haitian-vulnerable-children ;
    cacontology-institutional:receivesSupport geilenfeld-case:donor-manipulation-operation .

geilenfeld-case:haiti-location rdf:type uco-location:Location ;
    rdfs:label "Haiti"@en ;
    rdfs:comment "Republic of Haiti where St. Joseph's Home for Boys operated."@en ;
    uco-core:name "Haiti" .

geilenfeld-case:littleton-colorado-location rdf:type uco-location:Location ;
    rdfs:label "Littleton, Colorado"@en ;
    rdfs:comment "Most recent residence of Michael Karl Geilenfeld in Colorado, United States."@en ;
    uco-core:name "Littleton, Colorado" .

geilenfeld-case:united-states-location rdf:type uco-location:Location ;
    rdfs:label "United States"@en ;
    rdfs:comment "United States where Geilenfeld traveled from to Haiti for exploitation."@en ;
    uco-core:name "United States" .

# =============================================================================
# VULNERABLE POPULATION SERVED BY ORPHANAGE
# =============================================================================

geilenfeld-case:haitian-vulnerable-children rdf:type cacontology-institutional:VulnerableChildInCare ,
                                                    cacontology-institutional:OrphanedChild ,
                                                    cacontology-institutional:ImpoverishedChild ;
    rdfs:label "Haitian Vulnerable Children"@en ;
    rdfs:comment "Orphaned, impoverished, and otherwise vulnerable children served by St. Joseph's Home for Boys."@en .

# Specific victims who testified
geilenfeld-case:victim-1 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 1"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

geilenfeld-case:victim-2 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 2"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

geilenfeld-case:victim-3 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 3"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

geilenfeld-case:victim-4 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 4"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

geilenfeld-case:victim-5 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 5"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

geilenfeld-case:victim-6 rdf:type cacontology-institutional:VulnerableChildInCare ;
    rdfs:label "Victim 6"@en ;
    rdfs:comment "Child victim who testified at trial about exploitation by Geilenfeld."@en ;
    cacontology-institutional:providesTestimony geilenfeld-case:victim-testimony-coordination .

# =============================================================================
# INSTITUTIONAL EXPLOITATION PATTERNS
# =============================================================================

geilenfeld-case:orphanage-exploitation-operation rdf:type cacontology-institutional:OrphanageExploitation ,
                                                          cacontology-institutional:CharitableCoverExploitation ,
                                                          cacontology-institutional:LongTermInstitutionalControl ,
                                                          cacontology-institutional:VulnerablePopulationTargeting ,
                                                          cacontology-institutional:MultiModalInstitutionalAbuse ,
                                                          cacontology-institutional:SystematicInstitutionalAbuse ;
    rdfs:label "St. Joseph's Home Exploitation Operation"@en ;
    rdfs:comment "Systematic exploitation operation using orphanage as cover for repeated sexual, physical, and emotional abuse of vulnerable children over more than two decades."@en ;
    uco-core:startTime "1985-01-01T00:00:00Z"^^xsd:dateTime ; # Founded 1985
    uco-core:endTime "2010-12-31T23:59:59Z"^^xsd:dateTime ; # Charges cover 2005-2010
    cacontology-institutional:operationDurationYears 25 ;
    cacontology-institutional:victimCount 6 ; # 6 testified, likely more
    cacontology-institutional:exploitationTypeCount 3 ; # sexual, physical, emotional
    cacontology-institutional:vulnerabilityType "orphaned, impoverished, vulnerable" ;
    cacontology-institutional:trustLevelExploited "complete" ;
    cacontology-institutional:isolationDegree "complete" ;
    cacontology-institutional:occursWithin geilenfeld-case:st-josephs-home-for-boys ;
    cacontology-institutional:targetsPopulation geilenfeld-case:haitian-vulnerable-children ;
    cacontology-institutional:facilitatedBy geilenfeld-case:founder-director-role ;
    cacontology-institutional:impactsVictim geilenfeld-case:victim-1 ,
                                    geilenfeld-case:victim-2 ,
                                    geilenfeld-case:victim-3 ,
                                    geilenfeld-case:victim-4 ,
                                    geilenfeld-case:victim-5 ,
                                    geilenfeld-case:victim-6 ;
    cacontology-institutional:investigatedBy geilenfeld-case:hsi-investigation ,
                                      geilenfeld-case:fbi-investigation ;
    cacontology-institutional:prosecutedUnder geilenfeld-case:foreign-commerce-offense ,
                                       geilenfeld-case:foreign-place-offense .

# Specific Exploitation Methods
geilenfeld-case:position-of-trust-abuse rdf:type cacontology-institutional:PositionOfTrustAbuse ,
                                                 cacontology-institutional:InstitutionalAuthorityExploitation ,
                                                 cacontology-institutional:CareProviderExploitation ;
    rdfs:label "Position of Trust Abuse"@en ;
    rdfs:comment "Exploitation of position as trusted caregiver and authority figure responsible for children's welfare."@en ;
    cacontology-institutional:trustLevelExploited "complete" ;
    cacontology-institutional:exploitsRole geilenfeld-case:founder-director-role .

geilenfeld-case:isolation-based-control rdf:type cacontology-institutional:IsolationBasedControl ;
    rdfs:label "Isolation Based Control"@en ;
    rdfs:comment "Control method using complete isolation of children from family, community, and external oversight."@en ;
    cacontology-institutional:isolationDegree "complete" .

geilenfeld-case:dependency-exploitation rdf:type cacontology-institutional:DependencyExploitation ;
    rdfs:label "Dependency Exploitation"@en ;
    rdfs:comment "Exploitation of children's complete dependency on orphanage for food, shelter, safety, and care."@en .

# =============================================================================
# CROSS-BORDER TRAVEL FOR EXPLOITATION
# =============================================================================

geilenfeld-case:repeated-travel-pattern rdf:type cacontology-institutional:RepeatedCrossBorderTravel ,
                                                 cacontology-institutional:ForeignCommerceTravel ,
                                                 cacontology-institutional:HumanitarianTravelCover ,
                                                 cacontology-institutional:ForeignResidenceExploitation ;
    rdfs:label "Repeated US-Haiti Travel Pattern"@en ;
    rdfs:comment "Pattern of repeated travel from United States to Haiti over more than two decades specifically for exploitation of children at orphanage."@en ;
    uco-core:startTime "1985-01-01T00:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2010-12-31T23:59:59Z"^^xsd:dateTime ;
    cacontology-institutional:travelPatternYears 25 ;
    cacontology-institutional:travelFrequency "quarterly" ; # Estimated frequent travel
    cacontology-institutional:travelPurposeClaimed "humanitarian" ;
    cacontology-institutional:foreignResidenceDuration 300 ; # 25 years * 12 months
    cacontology-institutional:travelsFrom geilenfeld-case:united-states-location ;
    cacontology-institutional:travelsTo geilenfeld-case:haiti-location ;
    cacontology-institutional:enablesExploitation geilenfeld-case:orphanage-exploitation-operation ;
    cacontology-institutional:concealsTravel geilenfeld-case:humanitarian-cover-operation .

geilenfeld-case:humanitarian-cover-operation rdf:type cacontology-institutional:HumanitarianTravelCover ;
    rdfs:label "Humanitarian Cover Operation"@en ;
    rdfs:comment "Use of humanitarian orphanage work as cover for cross-border travel for exploitation purposes."@en ;
    cacontology-institutional:travelPurposeClaimed "humanitarian" ;
    cacontology-institutional:concealsTravel geilenfeld-case:repeated-travel-pattern .

# =============================================================================
# FINANCIAL MANIPULATION AND DONOR DECEPTION
# =============================================================================

geilenfeld-case:donor-manipulation-operation rdf:type cacontology-institutional:DonorManipulation ,
                                                      cacontology-institutional:CharitableFundingMisuse ,
                                                      cacontology-institutional:SupportNetworkDeception ,
                                                      cacontology-institutional:FinancialControlMechanism ;
    rdfs:label "Donor Manipulation Operation"@en ;
    rdfs:comment "Systematic manipulation of donors and supporters to maintain funding for abusive orphanage operations while concealing true nature of activities."@en ;
    cacontology-institutional:donorCount 50 ; # Estimated donor base
    cacontology-institutional:manipulationTactics "emotional appeals, false representation, concealment of abuse" ;
    cacontology-institutional:fundingAmount 500000.0 ; # Estimated funding over 25 years in USD
    cacontology-institutional:manipulates geilenfeld-case:us-supporters ;
    cacontology-institutional:concealsFrom geilenfeld-case:us-supporters .

geilenfeld-case:us-supporters rdf:type uco-identity:Person ;
    rdfs:label "US Supporters"@en ;
    rdfs:comment "US-based individuals and organizations who financially supported the orphanage believing it was legitimate humanitarian work."@en .

# =============================================================================
# LEGAL CHARGES AND PROSECUTION
# =============================================================================

geilenfeld-case:foreign-commerce-offense rdf:type cacontology-institutional:ForeignCommerceOffense ;
    rdfs:label "Foreign Commerce Offense"@en ;
    rdfs:comment "One count of traveling in foreign commerce for the purpose of engaging in illicit sexual conduct."@en ;
    cacontology-institutional:chargeCount 1 ;
    cacontology-institutional:prosecutionJurisdiction "US District Court, Southern District of Florida" .

geilenfeld-case:foreign-place-offense rdf:type cacontology-institutional:ForeignPlaceOffense ,
                                               cacontology-institutional:MultiVictimForeignOffense ;
    rdfs:label "Foreign Place Offense"@en ;
    rdfs:comment "Six counts of engaging in illicit sexual conduct in a foreign place, each relating to separate child victim."@en ;
    cacontology-institutional:chargeCount 6 ;
    cacontology-institutional:prosecutionJurisdiction "US District Court, Southern District of Florida" .

geilenfeld-case:us-prosecution-case rdf:type cacontology-institutional:USProsecutionForeignCrimes ;
    rdfs:label "US Prosecution of Geilenfeld"@en ;
    rdfs:comment "US prosecution of crimes committed by US citizen in Haiti, demonstrating extraterritorial jurisdiction."@en ;
    uco-core:startTime "2024-01-01T00:00:00Z"^^xsd:dateTime ; # Estimated prosecution start
    uco-core:endTime "2025-05-23T00:00:00Z"^^xsd:dateTime ; # Sentencing date
    cacontology-institutional:prosecutionJurisdiction "US District Court, Southern District of Florida" ;
    cacontology-institutional:sentenceLength 210 ; # 210 years
    cacontology-institutional:resultsInSentence geilenfeld-case:prison-sentence .

geilenfeld-case:prison-sentence rdf:type uco-observable:ObservableObject ;
    rdfs:label "210-Year Prison Sentence"@en ;
    rdfs:comment "210-year prison sentence imposed on May 23, 2025."@en ;
    uco-core:name "210-year prison sentence" .

# =============================================================================
# VICTIM TESTIMONY AND EVIDENCE COORDINATION
# =============================================================================

geilenfeld-case:victim-testimony-coordination rdf:type cacontology-institutional:MultipleVictimTestimony ,
                                                       cacontology-institutional:AdultVictimTestimony ;
    rdfs:label "Six-Victim Testimony Coordination"@en ;
    rdfs:comment "Coordination of testimony from six victims who testified about sexual abuse and its devastating impact."@en ;
    cacontology-institutional:victimTestimoniesCount 6 ;
    cacontology-institutional:yearsFromVictimizationToTestimony 15 ; # Estimated average time
    cacontology-institutional:revealsPattern geilenfeld-case:long-term-abuse-pattern .

geilenfeld-case:long-term-abuse-pattern rdf:type cacontology-institutional:LongTermAbusePattern ,
                                                 cacontology-institutional:InstitutionalAbuseEvidence ;
    rdfs:label "Long-Term Institutional Abuse Pattern"@en ;
    rdfs:comment "Evidence pattern showing systematic abuse spanning more than two decades within institutional setting."@en ;
    cacontology-institutional:abuseStartYear "1985"^^xsd:gYear ;
    cacontology-institutional:abuseEndYear "2010"^^xsd:gYear ;
    cacontology-institutional:documentsPattern geilenfeld-case:orphanage-exploitation-operation .

# Additional victim testimony evidence
geilenfeld-case:additional-victim-testimony rdf:type cacontology-institutional:AdultVictimTestimony ;
    rdfs:label "Additional Victim Testimony"@en ;
    rdfs:comment "Testimony from other victims (now adults) who were not subject of charged offenses but testified about abuse patterns."@en .

geilenfeld-case:witness-testimony-evidence rdf:type uco-observable:ObservableObject ;
    rdfs:label "Witness Testimony Evidence"@en ;
    rdfs:comment "Testimony from witnesses describing physical abuse and manipulation employed to keep operation running."@en .

geilenfeld-case:physical-abuse-evidence rdf:type cacontology-institutional:InstitutionalAbuseEvidence ;
    rdfs:label "Physical Abuse Evidence"@en ;
    rdfs:comment "Evidence of physical abuse inflicted on victims including physical assault and other forms of punishment."@en .

geilenfeld-case:emotional-abuse-evidence rdf:type cacontology-institutional:InstitutionalAbuseEvidence ;
    rdfs:label "Emotional Abuse Evidence"@en ;
    rdfs:comment "Evidence of emotional abuse and manipulation tactics used to control victims and maintain operation."@en .

# =============================================================================
# INVESTIGATION AGENCIES
# =============================================================================

geilenfeld-case:hsi-investigation rdf:type uco-identity:Organization ;
    rdfs:label "Homeland Security Investigations (HSI)"@en ;
    rdfs:comment "US Immigration and Customs Enforcement's Homeland Security Investigations unit that investigated the case."@en ;
    uco-core:name "HSI" .

geilenfeld-case:fbi-investigation rdf:type uco-identity:Organization ;
    rdfs:label "Federal Bureau of Investigation (FBI)"@en ;
    rdfs:comment "FBI investigation of the Geilenfeld case."@en ;
    uco-core:name "FBI" .

geilenfeld-case:southern-district-florida rdf:type uco-identity:Organization ;
    rdfs:label "US Attorney's Office, Southern District of Florida"@en ;
    rdfs:comment "US Attorney's Office that prosecuted the case."@en ;
    uco-core:name "USAO-SDFL" .

geilenfeld-case:ceos-prosecution rdf:type uco-identity:Organization ;
    rdfs:label "Child Exploitation and Obscenity Section (CEOS)"@en ;
    rdfs:comment "DOJ Criminal Division's Child Exploitation and Obscenity Section that handled prosecution."@en ;
    uco-core:name "CEOS" .

# =============================================================================
# PROJECT SAFE CHILDHOOD INTEGRATION
# =============================================================================

geilenfeld-case:project-safe-childhood rdf:type cacontology-prevention:ProjectSafeChildhood ;
    rdfs:label "Project Safe Childhood"@en ;
    rdfs:comment "Nationwide initiative launched in May 2006 to combat child sexual exploitation and abuse under which this case was brought."@en ;
    cacontology-prevention:initiativeName "Project Safe Childhood" ;
    cacontology-prevention:launchDate "2006-05-01T00:00:00Z"^^xsd:dateTime .

# Case Integration with Project Safe Childhood
geilenfeld-case:orphanage-exploitation-operation cacontology-prevention:prosecutedUnder geilenfeld-case:project-safe-childhood .

# =============================================================================
# CASE IMPACT AND SIGNIFICANCE
# =============================================================================

geilenfeld-case:devastating-victim-impact rdf:type uco-observable:ObservableObject ;
    rdfs:label "Devastating Victim Impact"@en ;
    rdfs:comment "Devastating impact on victims as described in their testimony at trial."@en .

geilenfeld-case:international-jurisdiction-precedent rdf:type uco-observable:ObservableObject ;
    rdfs:label "International Jurisdiction Precedent"@en ;
    rdfs:comment "Case demonstrates US ability to prosecute citizens for crimes committed abroad against children."@en .

geilenfeld-case:charitable-organization-exploitation-pattern rdf:type uco-observable:ObservableObject ;
    rdfs:label "Charitable Organization Exploitation Pattern"@en ;
    rdfs:comment "Pattern showing how charitable organizations can be used as cover for systematic child exploitation."@en .

