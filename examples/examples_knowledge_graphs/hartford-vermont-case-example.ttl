@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-location: <https://ontology.unifiedcyberontology.org/uco/location/> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action/> .
@prefix uco-observable: <https://ontology.unifiedcyberontology.org/uco/observable/> .
@prefix cacontology: <https://cacontology.projectvic.org#> .
@prefix cacontology-production: <https://cacontology.projectvic.org/production#> .
@prefix cacontology-custodial: <https://cacontology.projectvic.org/custodial#> .
@prefix cacontology-physical: <https://cacontology.projectvic.org/physical#> .
@prefix cacontology-sentencing: <https://cacontology.projectvic.org/sentencing#> .
@prefix cacontology-taskforce: <https://cacontology.projectvic.org/taskforce#> .

# =============================================================================
# Hartford, Vermont Production Case Example
# Case: Matthew Isaacs, charged with production of child sexual abuse materials
# Demonstrates proposed ontology enhancements for:
# - Image post-processing and digital modification
# - Residential proximity access patterns  
# - Identifying marks (tattoos) in evidence
# - Conditions of release violations
# - Project Safe Childhood integration
# =============================================================================

# Case Overview
<https://example.org/cases/hartford-vermont-production> rdf:type cacontology-production:ProductionOffense ;
    rdfs:label "Hartford Vermont Production Case"@en ;
    cacontology-production:productionMethod "photography_with_modification" ;
    cacontology-production:victimCount 1 ;
    cacontology-production:producedAt <https://example.org/locations/hartford-apartment> ;
    uco-core:startTime "2024-01-01T00:00:00Z"^^xsd:dateTime ;
    uco-core:endTime "2024-04-01T00:00:00Z"^^xsd:dateTime .

# Perpetrator
<https://example.org/persons/matthew-isaacs> rdf:type cacontology-production:Producer ;
    rdfs:label "Matthew Isaacs"@en ;
    uco-identity:hasName "Matthew Isaacs" ;
    uco-identity:age 33 ;
    uco-location:hasLocation <https://example.org/locations/hartford-apartment> .

# Victim (anonymized)
<https://example.org/persons/child-victim> rdf:type cacontology-production:ProductionVictim ;
    rdfs:label "Child Victim"@en ;
    uco-identity:age 5 ;
    uco-location:hasLocation <https://example.org/locations/hartford-apartment-same-floor> .

# Locations
<https://example.org/locations/hartford-apartment> rdf:type uco-location:Location ;
    rdfs:label "Hartford Apartment Building"@en ;
    uco-location:hasLocationName "Hartford, Vermont Apartment" .

<https://example.org/locations/hartford-apartment-same-floor> rdf:type uco-location:Location ;
    rdfs:label "Same Floor Apartment"@en ;
    uco-location:hasLocationName "Same Floor, Hartford Apartment Building" .

# =============================================================================
# PROPOSED ENHANCEMENT: RESIDENTIAL PROXIMITY ACCESS PATTERN
# =============================================================================

# Residential Proximity Access (NEW CLASS)
<https://example.org/access/residential-proximity> rdf:type cacontology-custodial:ResidentialProximityAccess ;
    rdfs:label "Residential Proximity Access"@en ;
    cacontology-custodial:proximityType "same_floor" ;
    cacontology-custodial:buildingType "apartment_building" ;
    cacontology-custodial:accessFrequency "regular" .

# Neighbor Relationship (NEW CLASS)
<https://example.org/relationships/neighbor> rdf:type cacontology-custodial:NeighborRelationship ;
    rdfs:label "Same Floor Neighbor Relationship"@en ;
    cacontology-custodial:relationshipType "neighbor" ;
    cacontology-custodial:residentialProximity "same_floor" ;
    cacontology-custodial:familiarityLevel "acquainted" ;
    cacontology-custodial:trustLevel "minimal" ;
    cacontology-custodial:accessLevel "unsupervised_opportunity" .

# Same Floor Access (NEW CLASS)
<https://example.org/access/same-floor> rdf:type cacontology-custodial:SameFloorAccess ;
    rdfs:label "Same Floor Access Opportunity"@en ;
    cacontology-custodial:floorNumber "same" ;
    cacontology-custodial:accessPattern "recurring" ;
    cacontology-custodial:supervisionLevel "none" .

# =============================================================================
# PROPOSED ENHANCEMENT: IMAGE POST-PROCESSING AND DIGITAL MODIFICATION
# =============================================================================

# Original Photography
<https://example.org/actions/initial-photography> rdf:type cacontology-production:RecordingAction ;
    rdfs:label "Initial Photography of Abuse"@en ;
    cacontology-production:recordingType "still_photography" ;
    cacontology-production:targetArea "genitalia" ;
    cacontology-production:recordingDevice <https://example.org/devices/mobile-phone> ;
    uco-core:performer <https://example.org/persons/matthew-isaacs> ;
    uco-core:object <https://example.org/persons/child-victim> .

# Image Post-Processing (NEW CLASS)
<https://example.org/actions/image-modification> rdf:type cacontology-production:ImagePostProcessing ;
    rdfs:label "Digital Image Modification"@en ;
    cacontology-production:modificationCount 5 ;
    cacontology-production:modificationSoftware "mobile_photo_editor" ;
    cacontology-production:originalImages <https://example.org/evidence/original-photos> ;
    cacontology-production:modifiedImages <https://example.org/evidence/modified-photos> .

# Text Overlay (NEW CLASS)
<https://example.org/actions/text-overlay> rdf:type cacontology-production:TextOverlay ;
    rdfs:label "Victim Name and Explicit Text Addition"@en ;
    cacontology-production:textType "victim_name_and_slang" ;
    cacontology-production:textContent "victim first name + slang term for vagina" ;
    cacontology-production:textPlacement "overlay_on_image" ;
    cacontology-production:textCount 2 .

# Graphic Enhancement (NEW CLASS)
<https://example.org/actions/graphic-enhancement> rdf:type cacontology-production:GraphicEnhancement ;
    rdfs:label "Explicit Graphic Addition"@en ;
    cacontology-production:graphicType "explicit_sexual_imagery" ;
    cacontology-production:graphicDescription "erect penis pointed at child vaginal area" ;
    cacontology-production:graphicPlacement "digitally_overlaid" ;
    cacontology-production:graphicCount 1 .

# =============================================================================
# PROPOSED ENHANCEMENT: IDENTIFYING MARKS IN EVIDENCE
# =============================================================================

# Distinctive Tattoo (NEW CLASS)
<https://example.org/evidence/distinctive-tattoo> rdf:type cacontology-physical:Tattoo ;
    rdfs:label "Distinctive Arm Tattoo"@en ;
    cacontology-physical:tattooLocation "arm" ;
    cacontology-physical:tattooDescription "distinctive identifying design" ;
    cacontology-physical:identificationValue "high" ;
    cacontology-physical:evidenceType "photographic_identification" ;
    cacontology-physical:visibleInEvidence true .

# Body Part Location (NEW CLASS)
<https://example.org/anatomy/arm-location> rdf:type cacontology-physical:BodyPartLocation ;
    rdfs:label "Arm Location"@en ;
    cacontology-physical:bodyPart "arm" ;
    cacontology-physical:visibility "visible_in_photos" ;
    cacontology-physical:identificationReliability "high" .

# Identifying Mark Evidence (NEW CLASS)
<https://example.org/evidence/identifying-mark> rdf:type cacontology-physical:IdentifyingMark ;
    rdfs:label "Perpetrator Identifying Mark"@en ;
    cacontology-physical:markType "tattoo" ;
    cacontology-physical:identificationConfidence 0.95 ;
    cacontology-physical:evidentialValue "perpetrator_identification" ;
    cacontology-physical:visibleInContent true .

# =============================================================================
# PROPOSED ENHANCEMENT: CONDITIONS OF RELEASE VIOLATIONS
# =============================================================================

# Prior State Charges
<https://example.org/charges/prior-state-charge> rdf:type cacontology-sentencing:StateCharge ;
    rdfs:label "Vermont Lewd and Lascivious Conduct Charge"@en ;
    cacontology-sentencing:chargeCode "Vermont_Lewd_Lascivious_Child" ;
    cacontology-sentencing:chargeDescription "Lewd and Lascivious Conduct with a Child" ;
    cacontology-sentencing:chargingJurisdiction "Vermont Superior Court" ;
    cacontology-sentencing:chargeStatus "pending" .

# Conditions of Release Status (NEW CLASS)
<https://example.org/status/conditions-of-release> rdf:type cacontology-sentencing:PendingChargeStatus ;
    rdfs:label "Conditions of Release Status"@en ;
    cacontology-sentencing:releaseConditions "court_imposed_conditions" ;
    cacontology-sentencing:statusAtTimeOfOffense "active" ;
    cacontology-sentencing:priorChargeType "state_child_abuse" ;
    cacontology-sentencing:complianceStatus "violated" .

# Conditions of Release Violation (NEW CLASS)
<https://example.org/violations/release-violation> rdf:type cacontology-sentencing:ConditionsOfReleaseViolation ;
    rdfs:label "Federal Offense While on State Release"@en ;
    cacontology-sentencing:violationType "new_federal_offense" ;
    cacontology-sentencing:priorCharge <https://example.org/charges/prior-state-charge> ;
    cacontology-sentencing:newOffense <https://example.org/cases/hartford-vermont-production> ;
    cacontology-sentencing:aggravatingFactor true .

# State to Federal Escalation (NEW CLASS)
<https://example.org/escalations/state-to-federal> rdf:type cacontology-sentencing:StateToFederalEscalation ;
    rdfs:label "State to Federal Charge Escalation"@en ;
    cacontology-sentencing:stateCharge "Lewd and Lascivious Conduct" ;
    cacontology-sentencing:federalCharge "Production of Child Sexual Abuse Materials" ;
    cacontology-sentencing:escalationReason "federal_jurisdiction_production" ;
    cacontology-sentencing:jurisdictionalComplexity "enhanced" .

# =============================================================================
# PROPOSED ENHANCEMENT: PROJECT SAFE CHILDHOOD INTEGRATION
# =============================================================================

# Project Safe Childhood Case (NEW CLASS)
<https://example.org/initiatives/project-safe-childhood> rdf:type cacontology-taskforce:ProjectSafeChildhoodCase ;
    rdfs:label "Project Safe Childhood Case"@en ;
    cacontology-taskforce:initiativeName "Project Safe Childhood" ;
    cacontology-taskforce:initiativeType "nationwide_federal" ;
    cacontology-taskforce:caseCategory "production_offense" ;
    cacontology-taskforce:federalInitiative true .

# Federal Initiative Participation (NEW CLASS)
<https://example.org/participation/federal-initiative> rdf:type cacontology-taskforce:FederalInitiativeParticipation ;
    rdfs:label "Federal Initiative Participation"@en ;
    cacontology-taskforce:participatingAgency "USAO Vermont" ;
    cacontology-taskforce:supportingAgency "Homeland Security Investigations" ;
    cacontology-taskforce:localAgency "Hartford Vermont Police Department" ;
    cacontology-taskforce:initiativeScope "national" .

# =============================================================================
# FEDERAL CHARGES AND SENTENCING
# =============================================================================

# Federal Production Charge
<https://example.org/charges/federal-production> rdf:type cacontology-sentencing:FederalCharge ;
    rdfs:label "Production of Child Sexual Abuse Materials"@en ;
    cacontology-sentencing:chargeCode "18_USC_2251" ;
    cacontology-sentencing:chargeDescription "Production of child sexual abuse materials" ;
    cacontology-sentencing:minimumSentence 180 ; # 15 years mandatory minimum
    cacontology-sentencing:maximumSentence 360 ; # 30 years maximum
    cacontology-sentencing:mandatoryMinimum true .

# Enhanced Sentencing Factors
<https://example.org/sentencing/enhanced-factors> rdf:type cacontology-sentencing:SentencingEnhancement ;
    rdfs:label "Enhanced Sentencing Factors"@en ;
    cacontology-sentencing:enhancementType "prior_state_charges" ;
    cacontology-sentencing:enhancementFactor "conditions_of_release_violation" ;
    cacontology-sentencing:enhancementSeverity "significant" ;
    cacontology-sentencing:imageModification true ;
    cacontology-sentencing:vulnerableVictim true .

# =============================================================================
# INVESTIGATION AND EVIDENCE
# =============================================================================

# Multi-Agency Investigation
<https://example.org/investigations/hartford-investigation> rdf:type cacontology:Investigation ;
    rdfs:label "Hartford Vermont Production Investigation"@en ;
    cacontology:investigationType "production_offense" ;
    cacontology:leadAgency "Hartford Vermont Police Department" ;
    cacontology:federalAgency "Homeland Security Investigations" ;
    cacontology:prosecutingOffice "USAO Vermont" .

# Digital Evidence
<https://example.org/evidence/modified-photos> rdf:type cacontology-production:ProducedImage ;
    rdfs:label "Modified Abuse Photographs"@en ;
    cacontology-production:imageCount 5 ;
    cacontology-production:modificationType "text_and_graphic_overlay" ;
    cacontology-production:evidenceValue "primary" ;
    cacontology-production:perpetratorVisible true ;
    cacontology-production:identifyingMarksVisible true .

<https://example.org/evidence/original-photos> rdf:type cacontology-production:ProducedImage ;
    rdfs:label "Original Abuse Photographs"@en ;
    cacontology-production:imageCount 5 ;
    cacontology-production:modificationType "none" ;
    cacontology-production:captureMethod "mobile_phone_camera" .

# Physical Evidence
<https://example.org/devices/mobile-phone> rdf:type cacontology-physical:MobileRecordingDevice ;
    rdfs:label "Perpetrator Mobile Phone"@en ;
    cacontology-physical:deviceType "smartphone" ;
    cacontology-physical:evidenceType "recording_device" ;
    cacontology-physical:containsEvidence true .

