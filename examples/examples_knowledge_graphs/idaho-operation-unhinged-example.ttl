# =============================================================================
# IDAHO OPERATION UNHINGED EXAMPLE
# Demonstrating specialized units, K9 programs, and enhanced coordination
# =============================================================================

# Idaho ICAC Task Force
ex:IdahoICAC rdf:type uco-identity:Organization ;
    rdfs:label "Idaho Internet Crimes Against Children Task Force"@en ;
    uco-core:name "Idaho ICAC Task Force" .

# Operation Unhinged
ex:OperationUnhinged rdf:type cacontology-specialized:OperationUnhinged ;
    rdfs:label "Operation Unhinged"@en ;
    cacontology-specialized:arrestCount 12 ;
    cacontology-multi:operationName "Operation Unhinged" ;
    cacontology-multi:operationPhase "completed" ;
    cacontology-multi:operationDuration 30.0 ;
    cacontology-multi:specializedUnitsDeployed 3 ;
    uco-action:performer ex:IdahoICAC .

# Operation Safe Online Summer (National Context)
ex:OperationSafeOnlineSummer rdf:type cacontology-multi:OperationSafeOnlineSummerType ;
    rdfs:label "Operation Safe Online Summer"@en ;
    cacontology-multi:operationName "Operation Safe Online Summer" ;
    cacontology-multi:taskForcesInvolved 61 ;
    cacontology-multi:operationPhase "ongoing" ;
    cacontology-multi:coordinationCenterLocation "National ICAC Coordination Center" .

# Badger the ESD K9
ex:BadgerTheESDK9 rdf:type cacontology-specialized:ElectronicStorageDeviceK9 ;
    rdfs:label "Badger the ESD K9"@en ;
    uco-core:name "Badger" ;
    cacontology-specialized:hasCapability ex:TripleThreatCapability .

# Triple Threat Capability
ex:TripleThreatCapability rdf:type cacontology-specialized:TripleThreatCapability ;
    rdfs:label "Triple Threat Capability"@en ;
    rdfs:comment "Electronic storage device detection, community engagement, and officer wellness support" .

# K9 Handler
ex:K9Handler001 rdf:type cacontology-specialized:K9Handler ;
    rdfs:label "K9 Handler"@en ;
    cacontology-specialized:operatesK9 ex:BadgerTheESDK9 .

# K9 Search Operations
ex:K9SearchOp001 rdf:type cacontology-specialized:K9SearchOperation ;
    rdfs:label "K9 Search Operation 1"@en ;
    cacontology-specialized:searchWarrantsExecuted 8 ;
    uco-action:performer ex:BadgerTheESDK9 .

