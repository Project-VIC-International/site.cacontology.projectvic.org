# =============================================================================
# ILLINOIS ATTORNEY GENERAL MACOUPIN COUNTY CASE
# Based on Illinois Attorney General Press Release, October 5, 2023
# Defendant: Robert L. Jones, 45, Macoupin County
# Multi-agency prosecution: Illinois AG + Macoupin County State's Attorney
# =============================================================================

# Case Investigation
:MacoupinCountyInvestigation rdf:type cacontology-core:ICACInvestigation ;
    uco-core:name "Illinois AG v. Robert L. Jones - Macoupin County CSAM Case"@en ;
    uco-core:description "Multi-agency investigation and prosecution of Macoupin County man for child pornography possession and dissemination, plus failure to register as sex offender."@en ;
    uco-core:startTime "2023-10-01T00:00:00Z"^^xsd:dateTime ;
    case-investigation:investigationStatus "prosecution_phase" .

# Defendant
:RobertLJones rdf:type uco-identity:Person ;
    uco-identity:fullName "Robert L. Jones"@en ;
    uco-core:name "Robert L. Jones"@en ;
    uco-identity:birthDate "1978-01-01"^^xsd:date ;  # Age 45 in 2023
    cacontology-registry:registeredSexOffender true .

:JonesResidence rdf:type uco-location:Location ;
    uco-location:addressLine1 "700 block South Street"@en ;
    uco-location:locality "Gillespie"@en ;
    uco-location:administrativeArea "Macoupin County"@en ;
    uco-location:country "United States"@en .

# =============================================================================
# ILLINOIS-SPECIFIC CRIMINAL CHARGES
# =============================================================================

# Class X Felony - Dissemination CSAM Under 13 (5 counts)
:DisseminationCharges rdf:type cacontology-sentencing:Illinois_DisseminationCSAM_Under13 ;
    uco-core:name "Dissemination of Child Pornography Under 13"@en ;
    cacontology-sentencing:chargeCount 5 ;
    cacontology-sentencing:maximumSentenceYears 30 ;
    cacontology-sentencing:victimAgeAtOffense 12 ;  # Under 13
    cacontology-sentencing:evidenceFoundOnDevice true ;
    cacontology-sentencing:evidenceFoundOnSocialMedia true .

# Class 1 Felony - Possession CSAM Under 13 (10 counts)
:PossessionCharges rdf:type cacontology-sentencing:Illinois_PossessionCSAM_Under13 ;
    uco-core:name "Possession of Child Pornography Under 13"@en ;
    cacontology-sentencing:chargeCount 10 ;
    cacontology-sentencing:maximumSentenceYears 15 ;
    cacontology-sentencing:victimAgeAtOffense 12 ;  # Under 13
    cacontology-sentencing:evidenceFoundOnDevice true ;
    cacontology-sentencing:evidenceFoundOnSocialMedia true .

# Class 3 Felony - Failure to Register (3 counts)
:FailureToRegisterCharges rdf:type cacontology-sentencing:Illinois_FailureToRegister ;
    uco-core:name "Failure to Register Under Sex Offender Act"@en ;
    cacontology-sentencing:chargeCount 3 ;
    cacontology-sentencing:maximumSentenceYears 7 .

# Link defendant to charges
:RobertLJones cacontology-sentencing:chargedWith :DisseminationCharges ,
                                         :PossessionCharges ,
                                         :FailureToRegisterCharges .

# =============================================================================
# MULTI-AGENCY PROSECUTION FRAMEWORK
# =============================================================================

# Illinois Attorney General Office
:IllinoisAttorneyGeneralOffice rdf:type cacontology-sentencing:StateAttorneyGeneralOffice ;
    uco-core:name "Illinois Attorney General Office"@en ;
    uco-identity:organization "Office of Illinois Attorney General Kwame Raoul"@en .

# High Tech Crimes Bureau
:HighTechCrimesBureau rdf:type cacontology-sentencing:HighTechCrimesBureau ;
    uco-core:name "High Tech Crimes Bureau"@en ;
    uco-core:description "Specialized prosecution unit for technology-related crimes within Illinois AG office."@en .

# Macoupin County State's Attorney Office
:MacoupinCountyStatesAttorneyOffice rdf:type cacontology-sentencing:CountyStatesAttorneyOffice ;
    uco-core:name "Macoupin County State's Attorney Office"@en ;
    uco-identity:organization "Jordan Garrison, Macoupin County State's Attorney"@en .

# Assistant Attorney General handling case
:ShantikumarKulkarni rdf:type cacontology-sentencing:AssistantAttorneyGeneral ;
    uco-identity:fullName "Shantikumar Kulkarni"@en ;
    uco-core:name "Assistant Attorney General Shantikumar Kulkarni"@en .

# Co-Prosecution between Illinois AG and County
:CoProsecutionEffort rdf:type cacontology-sentencing:CoProsecution ;
    uco-core:name "Illinois AG - Macoupin County Co-Prosecution"@en ;
    cacontology-sentencing:leadProsecutor :IllinoisAttorneyGeneralOffice ;
    cacontology-sentencing:supportingProsecutor :MacoupinCountyStatesAttorneyOffice ;
    cacontology-sentencing:coProsecutedBy :IllinoisAttorneyGeneralOffice ,
                                   :MacoupinCountyStatesAttorneyOffice ;
    cacontology-sentencing:handledByBureau :HighTechCrimesBureau .

# Link charges to prosecution type
:DisseminationCharges cacontology-sentencing:prosecutionType :CoProsecutionEffort .
:PossessionCharges cacontology-sentencing:prosecutionType :CoProsecutionEffort .
:FailureToRegisterCharges cacontology-sentencing:prosecutionType :CoProsecutionEffort .

