    uco-location:administrativeArea "Macoupin County"@en .

# Illinois ICAC Task Force
:IllinoisICACtaskForce rdf:type cacontology-taskforce:IllinoisICACtaskForce ;
    uco-core:name "Illinois ICAC Task Force"@en ;
    cacontology-taskforce:dojGrantFunded true ;
    cacontology-taskforce:nationalTaskForceNetwork true ;
    cacontology-taskforce:taskForceNumber 1 ;  # 1 of 61 nationwide
    cacontology-taskforce:federalPartner :FederalAgencies ;
    cacontology-taskforce:localPartner :GillespiePolice ,
                                :MacoupinCountySheriff .

