@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://example.org/international-coordination#> .

# Import all required ontologies
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action/> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-location: <https://ontology.unifiedcyberontology.org/uco/location/> .

@prefix cacontology: <https://cacontology.projectvic.org#> .
@prefix cacontology-international: <https://cacontology.projectvic.org/international#> .
@prefix cacontology-training: <https://cacontology.projectvic.org/training#> .
@prefix cacontology-prevention: <https://cacontology.projectvic.org/prevention#> .
@prefix cacontology-legal: <https://cacontology.projectvic.org/legal-harmonization#> .

# =============================================================================
# GLOBAL PARTNERSHIP FRAMEWORK
# =============================================================================

ex:ICMECPartnership rdf:type cacontology-international:InternationalPartnership ;
    cacontology-international:partnerCountryCount 120 ;
    cacontology-international:globalCoverage "worldwide" ;
    uco-core:name "ICMEC Global Partnership Network" ;
    uco-core:description "International Centre for Missing & Exploited Children partnership network spanning 120+ countries" .

ex:GlobalMissingChildrenCenter001 rdf:type cacontology-international:GlobalMissingChildrenCenter ;
    cacontology-international:managedBy ex:ICMEC ;
    cacontology-international:coordinatesWith ex:USTaskForce, ex:EuropeanTaskForce, ex:AsianTaskForce .

ex:ICMEC rdf:type uco-identity:Organization ;
    uco-core:name "International Centre for Missing & Exploited Children" .

ex:USTaskForce rdf:type uco-identity:Organization ;
    uco-core:name "US ICAC Task Force Network" .

ex:EuropeanTaskForce rdf:type uco-identity:Organization ;
    uco-core:name "European ICAC Task Force" .

ex:AsianTaskForce rdf:type uco-identity:Organization ;
    uco-core:name "Asian Pacific ICAC Coalition" .

# =============================================================================
# CROSS-BORDER INVESTIGATION EXAMPLE
# =============================================================================

ex:CrossBorderInvestigation001 rdf:type cacontology-international:CrossBorderInvestigation ;
    cacontology-international:jurisdictionCount 4 ;
    cacontology-international:coordinationComplexity "high" ;
    cacontology-international:operatesInCountry ex:USLocation, ex:UKLocation, ex:AustraliaLocation, ex:CanadaLocation ;
    cacontology-international:supportedBy ex:InternationalTaskForce001 .

ex:USLocation rdf:type uco-location:Location ;
    uco-core:name "United States" .

ex:UKLocation rdf:type uco-location:Location ;
    uco-core:name "United Kingdom" .

ex:AustraliaLocation rdf:type uco-location:Location ;
    uco-core:name "Australia" .

ex:CanadaLocation rdf:type uco-location:Location ;
    uco-core:name "Canada" .

ex:InternationalTaskForce001 rdf:type cacontology-international:InternationalTaskForce ;
    uco-core:name "Five Eyes ICAC Task Force" ;
    cacontology-international:coordinatesWith ex:USTaskForce, ex:UKTaskForce, ex:AustraliaTaskForce, ex:CanadaTaskForce .

# =============================================================================
# INFORMATION SHARING AND COOPERATION
# =============================================================================

ex:InformationSharingAgreement001 rdf:type cacontology-international:InformationSharingAgreement ;
    cacontology-international:multilateralAgreement true ;
    cacontology-international:informationSharingLevel "full" ;
    cacontology-international:sharesDataWith ex:USTaskForce, ex:UKTaskForce, ex:AustraliaTaskForce, ex:CanadaTaskForce .

ex:SecureChannel001 rdf:type cacontology-international:SecureChannel ;
    uco-core:name "ICAC Secure Communication Network" ;
    uco-core:description "Encrypted communication channel for international task force coordination" .

ex:InternationalAlert001 rdf:type cacontology-international:InternationalAlert ;
    cacontology-international:triggersAlert ex:CrossBorderInvestigation001 ;
    uco-core:description "Alert triggered for cross-border CSAM production network" .

# =============================================================================
# GLOBAL TRAINING PROGRAMS
# =============================================================================

ex:GlobalTrainingReach001 rdf:type cacontology-training:GlobalTrainingReach ;
    cacontology-training:professionalsTrained 155000 ;
    cacontology-international:globalCoverage "worldwide" ;
    uco-core:description "ICMEC global training reach across 120+ countries" .

ex:InternationalTraining001 rdf:type cacontology-training:InternationalTraining ;
    cacontology-training:trainingReachCount 2500 ;
    cacontology-training:trainingDuration 40.0 ;
    cacontology-training:completionRate 0.92 ;
    cacontology-training:trainingLanguage "English, Spanish, French, German, Japanese" ;
    cacontology-training:deliveredBy ex:InternationalTrainer001 ;
    cacontology-training:supportedBy ex:CapacityBuildingPartner001 .

ex:InternationalTrainer001 rdf:type cacontology-training:InternationalTrainer ;
    uco-core:name "Senior International Training Specialist" .

ex:CapacityBuildingPartner001 rdf:type cacontology-training:CapacityBuildingPartner ;
    uco-core:name "Global Capacity Building Consortium" ;
    cacontology-training:buildsCapacityFor ex:DevelopingCountryOrg001, ex:DevelopingCountryOrg002 .

ex:DevelopingCountryOrg001 rdf:type uco-identity:Organization ;
    uco-core:name "Developing Country Law Enforcement Agency A" .

ex:DevelopingCountryOrg002 rdf:type uco-identity:Organization ;
    uco-core:name "Developing Country Law Enforcement Agency B" .

# =============================================================================
# PROFESSIONAL CERTIFICATION PROGRAM
# =============================================================================

ex:ProfessionalCertification001 rdf:type cacontology-training:ProfessionalCertification ;
    cacontology-training:certificationStatus "active" ;
    cacontology-training:certificationValidityPeriod 36.0 ;
    cacontology-training:certifiedBy ex:CertificationBody001 .

ex:CertificationBody001 rdf:type cacontology-training:CertificationBody ;
    uco-core:name "International Child Protection Certification Authority" .

ex:CompetencyAssessment001 rdf:type cacontology-training:CompetencyAssessment ;
    cacontology-training:competencyLevel "expert" ;
    cacontology-training:assessedBy ex:TrainingParticipant001 .

ex:TrainingParticipant001 rdf:type cacontology-training:TrainingParticipant ;
    cacontology-training:participatedIn ex:InternationalTraining001 .

# =============================================================================
# GLOBAL PREVENTION INITIATIVES
# =============================================================================

ex:GlobalEducationPortal001 rdf:type cacontology-prevention:EducationPortal ;
    cacontology-prevention:educationPortalReach 50000 ;
    cacontology-prevention:languageAvailability "English, Spanish, French, German, Chinese, Arabic, Russian" ;
    cacontology-prevention:providesResource ex:PreventionMaterial001, ex:SafetyGuideline001 .

ex:PreventionMaterial001 rdf:type cacontology-prevention:PreventionMaterial ;
    cacontology-prevention:targetAudience "educators, parents, children" ;
    cacontology-prevention:languageAvailability "multilingual" ;
    cacontology-prevention:educatesOn ex:OnlineSafetyEducation001 .

ex:SafetyGuideline001 rdf:type cacontology-prevention:SafetyGuideline ;
    cacontology-prevention:targetAudience "schools, communities" ;
    cacontology-prevention:implementationStatus "active" .

ex:OnlineSafetyEducation001 rdf:type cacontology-prevention:OnlineSafetyEducation ;
    cacontology-prevention:ageGroup "6-18" ;
    cacontology-prevention:programDuration 12.0 ;
    cacontology-prevention:participantCount 25000 .

ex:CommunityOutreach001 rdf:type cacontology-prevention:CommunityOutreach ;
    cacontology-prevention:communityParticipation 0.78 ;
    cacontology-prevention:coordinatedBy ex:SafetyCoordinator001 ;
    cacontology-prevention:engagesCommunity ex:LocalCommunityOrg001, ex:LocalCommunityOrg002 .

ex:SafetyCoordinator001 rdf:type cacontology-prevention:SafetyCoordinator ;
    uco-core:name "International Safety Program Coordinator" .

ex:LocalCommunityOrg001 rdf:type uco-identity:Organization ;
    uco-core:name "Local Community Organization - Region A" .

ex:LocalCommunityOrg002 rdf:type uco-identity:Organization ;
    uco-core:name "Local Community Organization - Region B" .

# =============================================================================
# LEGAL HARMONIZATION AND COMPLIANCE
# =============================================================================

ex:GlobalLegalReview001 rdf:type cacontology-legal:GlobalLegalReview ;
    cacontology-legal:countriesAnalyzed 196 ;
    uco-core:name "CSAM Model Law Global Review 10th Edition" ;
    uco-core:description "Comprehensive analysis of child sexual abuse material legislation in 196 countries" .

ex:CSAMModelLaw001 rdf:type cacontology-legal:CSAMModelLaw ;
    uco-core:name "ICMEC CSAM Model Legislation" ;
    uco-core:description "Model legislation for Child Sexual Abuse Material regulations" .

ex:LegislativeAssessment001 rdf:type cacontology-legal:LegislativeAssessment ;
    cacontology-legal:legislativeCompliance 0.85 ;
    cacontology-legal:assessedBy ex:NationalLegislation001 ;
    cacontology-legal:identifiesGap ex:LegalFrameworkGap001 .

ex:NationalLegislation001 rdf:type cacontology-legal:NationalLegislation ;
    cacontology-legal:implementationStatus "enforced" ;
    cacontology-legal:legalSystemType "common_law" ;
    cacontology-legal:appliesToCountry ex:USLocation ;
    cacontology-legal:basedOnModel ex:CSAMModelLaw001 .

ex:LegalFrameworkGap001 rdf:type cacontology-legal:LegalFrameworkGap ;
    cacontology-legal:policyGapSeverity "moderate" ;
    cacontology-legal:requiresReform ex:LegalReform001 .

ex:LegalReform001 rdf:type cacontology-legal:LegalReform ;
    cacontology-legal:reformPriority "high" ;
    uco-core:description "Reform to address online grooming legislation gaps" .

# =============================================================================
# POLICY HARMONIZATION PROGRESS
# =============================================================================

ex:PolicyHarmonization001 rdf:type cacontology-legal:PolicyHarmonization ;
    uco-core:description "Harmonization of child protection policies across participating countries" .

ex:HarmonizationProgress001 rdf:type cacontology-legal:HarmonizationProgress ;
    cacontology-legal:harmonizationProgress 0.72 ;
    uco-core:description "72% progress toward policy harmonization across partner countries" .

ex:TechnicalAssistance001 rdf:type cacontology-legal:TechnicalAssistance ;
    cacontology-legal:providesAssistance ex:PolicyDevelopment001 ;
    uco-core:description "Technical assistance for developing comprehensive child protection legislation" .

ex:PolicyDevelopment001 rdf:type cacontology-legal:PolicyDevelopment ;
    uco-core:description "Development of harmonized child protection policies" .

# =============================================================================
# MUTUAL LEGAL ASSISTANCE
# =============================================================================

ex:MutualLegalAssistance001 rdf:type cacontology-legal:MutualLegalAssistance ;
    cacontology-legal:treatyBasis "Mutual Legal Assistance Treaty" ;
    cacontology-legal:facilitatesCooperation ex:InternationalLawEnforcement001 .

ex:InternationalLawEnforcement001 rdf:type cacontology-legal:InternationalLawEnforcement ;
    uco-core:description "Coordinated law enforcement action across multiple jurisdictions" .

ex:ExtraditionAgreement001 rdf:type cacontology-legal:ExtraditionAgreement ;
    cacontology-legal:enablesExtradition ex:InternationalLawEnforcement001 ;
    uco-core:description "Bilateral extradition agreement for child protection crimes" .

# =============================================================================
# GLOBAL HOTLINE NETWORK
# =============================================================================

ex:GlobalHotlineNetwork001 rdf:type cacontology-international:GlobalHotlineNetwork ;
    cacontology-international:managedBy ex:ICMEC ;
    cacontology-international:coordinatesHotlines ex:USHotline, ex:EuropeanHotline, ex:AsianHotline .

ex:USHotline rdf:type uco-identity:Organization ;
    uco-core:name "US National Center for Missing & Exploited Children" .

ex:EuropeanHotline rdf:type uco-identity:Organization ;
    uco-core:name "INHOPE European Hotline Network" .

ex:AsianHotline rdf:type uco-identity:Organization ;
    uco-core:name "Asian Hotline Coalition" .

ex:MultilingualSupport001 rdf:type cacontology-international:MultilingualSupport ;
    cacontology-international:languageSupport "English, Spanish, French, German, Chinese, Arabic, Russian, Portuguese, Japanese, Hindi" .

ex:CrossReferralProtocol001 rdf:type cacontology-international:CrossReferralProtocol ;
    uco-core:description "Protocol for referring reports between national hotlines based on jurisdiction and expertise" .

# =============================================================================
# INTERNATIONAL REFERRAL WORKFLOW
# =============================================================================

ex:InternationalReferral001 rdf:type cacontology-international:InternationalReferral ;
    cacontology-international:responseTimeInternational 24.0 ;
    cacontology-international:referredTo ex:EuropeanTaskForce ;
    uco-core:description "Referral of complex CSAM case from US to European partners for local investigation" .

ex:GlobalCaseTracking001 rdf:type cacontology-international:GlobalCaseTracking ;
    uco-core:description "Global system for tracking cross-border cases and coordination status" .

# =============================================================================
# EFFECTIVENESS METRICS
# =============================================================================

ex:InternationalImpact001 rdf:type cacontology-international:InternationalImpact ;
    uco-core:description "Assessment of international program effectiveness and global coordination impact" .

ex:CrossBorderEffectiveness001 rdf:type cacontology-international:CrossBorderEffectiveness ;
    uco-core:description "Measurement of effectiveness in cross-border investigation coordination" .

ex:TrainingEffectiveness001 rdf:type cacontology-training:TrainingEffectiveness ;
    cacontology-training:trainingEffectiveness 0.88 ;
    uco-core:description "88% effectiveness rating for international training programs" .

ex:PreventionEffectiveness001 rdf:type cacontology-prevention:PreventionEffectiveness ;
    cacontology-prevention:preventionEffectiveness 0.75 ;
    uco-core:description "75% effectiveness rating for global prevention initiatives" .

ex:LegalEffectiveness001 rdf:type cacontology-legal:LegalEffectiveness ;
    cacontology-legal:effectivenessScore 0.80 ;
    uco-core:description "80% effectiveness score for legal harmonization efforts" .

# =============================================================================
# COORDINATION RELATIONSHIPS
# =============================================================================

ex:ICMECPartnership cacontology-international:coordinatesWith ex:ICMEC .
ex:CrossBorderInvestigation001 cacontology-international:requiresAssistance ex:MutualLegalAssistance001 .
ex:InternationalTraining001 cacontology-training:improvesProficiency ex:CrossBorderInvestigation001 .
ex:GlobalEducationPortal001 cacontology-prevention:preventsMechanism ex:OnlineSafetyEducation001 .
ex:PolicyHarmonization001 cacontology-legal:facilitatesCooperation ex:InternationalLawEnforcement001 . 