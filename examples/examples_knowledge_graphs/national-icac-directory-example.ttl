@prefix example: <http://ontology.icac.org/examples/national-cacontology-directory#> .
@prefix cacontology-core: <http://ontology.icac.org/cacontology-core#> .
@prefix cacontology-taskforce: <http://ontology.icac.org/cacontology-taskforce#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-location: <https://ontology.unifiedcyberontology.org/uco/location/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# =============================================================================
# NATIONAL ICAC TASK FORCE DIRECTORY (Based on icactaskforce.org)
# =============================================================================

example:NationalICACtaskForceDirectory_2025 rdf:type cacontology-taskforce:NationalICACtaskForceDirectory ;
    cacontology-taskforce:totalTaskForces 61 ;
    cacontology-taskforce:statesWithMultipleTaskForces 6 ;
    cacontology-taskforce:nationalCoveragePercentage 100.0 ;
    rdfs:label "National ICAC Task Force Directory 2025"@en ;
    rdfs:comment "Complete directory of all 61 ICAC task forces in the United States as of 2025."@en .

# =============================================================================
# MARYLAND STATE POLICE HOST ORGANIZATION
# =============================================================================

example:MarylandStatePolice rdf:type cacontology-taskforce:StatePoliceHost ;
    cacontology-taskforce:hostOrganizationType "state_police" ;
    cacontology-taskforce:hostJurisdictionLevel "state" ;
    cacontology-taskforce:organizationName "Maryland State Police" ;
    uco-core:name "Maryland State Police" .

example:MarylandTaskForce rdf:type cacontology-taskforce:StatewideTaskForce ;
    cacontology-taskforce:coverageType "statewide" ;
    cacontology-taskforce:jurisdictionPopulation 6177224 ; # Maryland population
    cacontology-taskforce:hostedBy example:MarylandStatePolice ;
    cacontology-taskforce:hasContactInformation example:MarylandContactInfo .

example:MarylandContactInfo rdf:type cacontology-taskforce:TaskForceContactInformation ;
    cacontology-taskforce:phoneNumber "410-694-4706" ;
    cacontology-taskforce:emailAddress "msp.icac@maryland.gov" .

# =============================================================================
# CALIFORNIA MULTI-REGIONAL SYSTEM (5 Task Forces)
# =============================================================================

example:CaliforniaState rdf:type cacontology-taskforce:CaliforniaRegionalSystem ;
    cacontology-taskforce:regionalTaskForceCount 5 ;
    rdfs:label "California Multi-Regional ICAC System"@en .

# Los Angeles Area Task Force
example:LAPD rdf:type cacontology-taskforce:LocalPoliceHost ;
    cacontology-taskforce:hostOrganizationType "local_police" ;
    cacontology-taskforce:organizationName "Los Angeles Police Department" .

example:LATaskForce rdf:type cacontology-taskforce:MetropolitanTaskForce ;
