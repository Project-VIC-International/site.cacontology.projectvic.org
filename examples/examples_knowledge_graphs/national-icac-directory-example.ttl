# =============================================================================
# DIRECTORY RELATIONSHIPS
# =============================================================================

# National directory includes all task forces
example:NationalICACtaskForceDirectory_2025 cacontology-taskforce:includesTaskForce 
    example:MarylandTaskForce,
    example:LATaskForce,
    example:SanDiegoTaskForce,
    example:FresnoTaskForce,
    example:TexasStatewideTaskForce,
    example:DallasTaskForce,
    example:HoustonTaskForce,
    example:FloridaCentralTaskForce,
    example:FloridaSouthernTaskForce,
    example:FloridaNorthernTaskForce,
    example:MilitaryTaskForce,
    example:GeorgiaTaskForce,
    example:NorthCarolinaTaskForce,
    example:PennsylvaniaTaskForce .

# Multi-regional state relationships
example:CaliforniaState cacontology-taskforce:hasRegionalTaskForce 
    example:LATaskForce,
    example:SanDiegoTaskForce,
    example:FresnoTaskForce .

example:TexasState cacontology-taskforce:hasRegionalTaskForce 
    example:TexasStatewideTaskForce,
    example:DallasTaskForce,
    example:HoustonTaskForce .

example:FloridaState cacontology-taskforce:hasRegionalTaskForce 
    example:FloridaCentralTaskForce,
    example:FloridaSouthernTaskForce,
    example:FloridaNorthernTaskForce .

# California regional coordination
example:LATaskForce cacontology-taskforce:coordinatesWith example:SanDiegoTaskForce .
example:LATaskForce cacontology-taskforce:coordinatesWith example:FresnoTaskForce .
example:SanDiegoTaskForce cacontology-taskforce:coordinatesWith example:FresnoTaskForce .

# Texas regional coordination
example:TexasStatewideTaskForce cacontology-taskforce:coordinatesWith example:DallasTaskForce .
example:TexasStatewideTaskForce cacontology-taskforce:coordinatesWith example:HoustonTaskForce .
example:DallasTaskForce cacontology-taskforce:coordinatesWith example:HoustonTaskForce .

# Military coordination with civilian task forces
example:MilitaryTaskForce cacontology-taskforce:coordinatesWithCivilian example:MarylandTaskForce .
example:MilitaryTaskForce cacontology-taskforce:coordinatesWithCivilian example:TexasStatewideTaskForce .

# National hotline accessibility
example:MarylandTaskForce cacontology-taskforce:accessibleVia example:NationalICACtHotline .
example:LATaskForce cacontology-taskforce:accessibleVia example:NationalICACtHotline .
example:MilitaryTaskForce cacontology-taskforce:accessibleVia example:NationalICACtHotline . 