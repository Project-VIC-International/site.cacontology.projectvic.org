    cacontology-taskforce:metropolitanArea "Los Angeles" ;
    cacontology-taskforce:regionCovered "Los Angeles Area" ;
    cacontology-taskforce:hostedBy example:LAPD ;
    cacontology-taskforce:hasContactInformation example:LAContactInfo .

example:LAContactInfo rdf:type cacontology-taskforce:TaskForceContactInformation ;
    cacontology-taskforce:phoneNumber "562-254-0637" .

