    cacontology-corruption:employmentStatus "reserve" .

ex:uniform-enhanced-production rdf:type cacontology-corruption:UniformEnhancedProduction ;
    rdfs:label "Uniform Enhanced CSAM Production"@en ;
    rdfs:comment "Production of CSAM enhanced by wearing official uniforms."@en ;
    cacontology-corruption:uniformDisplayed true ;
    cacontology-corruption:wearsUniform ex:minnesota-state-trooper-uniform, ex:us-army-uniform .

