    cacontology-corruption:corruptionDuration 8 ;
    cacontology-corruption:victimCount 2 ;
    cacontology-corruption:uniformDisplayed true ;
    cacontology-corruption:badgeVisible true ;
    cacontology-corruption:exploitsPosition ex:state-trooper-role, ex:army-reservist-role ;
    cacontology-corruption:wearsUniform ex:minnesota-state-trooper-uniform, ex:us-army-uniform ;
    cacontology-corruption:investigatedBy ex:fbi-minneapolis .

ex:corrupt-state-trooper rdf:type cacontology-corruption:CorruptStateTrooper ;
    rdfs:label "Corrupt Minnesota State Trooper"@en ;
    rdfs:comment "Minnesota state trooper who produced CSAM while in uniform."@en ;
