    cacontology-corruption:authorityLevel "patrol" ;
    cacontology-corruption:departmentAffiliation "Minnesota State Patrol" ;
    cacontology-corruption:employmentStatus "active" .

ex:corrupt-army-reservist rdf:type cacontology-corruption:CorruptArmyReservist ;
    rdfs:label "Corrupt Army Reservist"@en ;
