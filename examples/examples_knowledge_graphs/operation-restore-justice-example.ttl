# =============================================================================
# OPERATION RESTORE JUSTICE EXAMPLE
# Demonstrates nationwide FBI coordination, law enforcement corruption, 
# community outreach effectiveness, and rapid response capabilities
# =============================================================================

# =============================================================================
# OPERATION COORDINATION
# =============================================================================

ex:operation-restore-justice rdf:type cacontology-multi:AllFBIFieldOfficesOperation, cacontology-multi:CEOSCoordinatedOperation, cacontology-multi:ProjectSafeChildhoodOperation ;
    rdfs:label "Operation Restore Justice"@en ;
    rdfs:comment "5-day nationwide coordinated enforcement effort involving all 55 FBI field offices."@en ;
    cacontology-multi:operationName "Operation Restore Justice" ;
    cacontology-multi:operationDurationDays 5 ;
    cacontology-multi:fbiFieldOfficesInvolved 55 ;
    cacontology-multi:arrestsNationwide 205 ;
    cacontology-multi:childrenRescuedCount 115 ;
    cacontology-multi:operationStartDate "2025-04-14T00:00:00Z"^^xsd:dateTime ;
    cacontology-multi:operationEndDate "2025-04-18T23:59:59Z"^^xsd:dateTime ;
    cacontology-multi:coordinatesNationwide ex:child-exploitation-obscenity-section ;
    cacontology-multi:involvesFBIFieldOffice ex:fbi-minneapolis, ex:fbi-norfolk, ex:fbi-washington-dc, ex:fbi-california ;
    cacontology-multi:hostedBy ex:department-of-justice .

ex:mass-child-rescue rdf:type cacontology-multi:NationwideChildRescueCoordination ;
    rdfs:label "Operation Restore Justice Child Rescue"@en ;
    rdfs:comment "Coordination resulting in rescue of 115 children nationwide."@en ;
    cacontology-multi:childrenRescuedCount 115 ;
    cacontology-multi:rescuesChildren ex:rescued-child-1, ex:rescued-child-2, ex:rescued-child-3 .

ex:child-abuse-prevention-month-coordination rdf:type cacontology-prevention:ChildAbusePreventionMonth ;
    rdfs:label "April 2025 Child Abuse Prevention Month"@en ;
    rdfs:comment "National Child Abuse Prevention Month awareness activities coordinated with Operation Restore Justice."@en ;
    cacontology-prevention:campaignMonth "April" ;
    cacontology-prevention:coordinatedWith ex:department-of-justice, ex:project-safe-childhood .

