    cacontology-registry:fullName "Nicolas James Palmisano" ;
    cacontology-registry:race "White" ; # Estimated
    cacontology-registry:sex "Male" ;
    cacontology-registry:tier "Tier II" ; # Estimated based on prior juvenile offense
    cacontology-registry:priorConvictionCount 1 ;
    cacontology-registry:yearsBetweenOffenses 5.0 ; # 2019 conviction, released ~2023, 2024 new offense
    cacontology-registry:sentenceServed "4 years prison" ;
