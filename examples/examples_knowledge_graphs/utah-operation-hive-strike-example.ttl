# =============================================================================
# OPERATION HIVE STRIKE - UTAH ICAC TASK FORCE (April 14-17, 2025)
# =============================================================================

# Operation Definition
ex:OperationHiveStrike rdf:type cacontology-multi:TaskForceHostedOperation ,
                                gufo:Event ;
    rdfs:label "Operation Hive Strike"@en ;
    rdfs:comment "Utah ICAC Task Force statewide undercover investigation targeting online predators and CSAM distributors across 5 counties."@en ;
    cacontology-multi:operationName "Operation Hive Strike" ;
    cacontology-multi:operationBranding "branded" ;
    cacontology-multi:operationDurationDays 4.0 ;
    cacontology-multi:statewideCoverage true ;
    cacontology-multi:totalArrests 15 ;
    cacontology-multi:searchWarrantsExecuted 10 ;
    cacontology-multi:countiesInvolved 5 ;
    cacontology-multi:targetTypeCount 2 ;
    cacontology-multi:targetStrategy "simultaneous" ;
    cacontology-multi:chargeTypesDiversity 8 ;
    cacontology-multi:hostedBy ex:UtahICACtaskForce ;
    cacontology-multi:coversState ex:UtahState ;
    cacontology-multi:spansCounties ex:DavisCounty, ex:SaltLakeCounty, ex:SummitCounty, ex:UtahCounty, ex:WeberCounty ;
    cacontology-multi:utilizesCoordination ex:HiveStrikeCoordination .

# Dual Target Operation Classification
ex:OperationHiveStrike rdf:type cacontology-multi:DualTargetOperation ;
    cacontology-multi:targetsOffenderType ex:OnlinePredatorRole, ex:CSAMDistributorRole .

# =============================================================================
# HOSTING TASK FORCE AND COORDINATION
# =============================================================================

# Utah ICAC Task Force
ex:UtahICACtaskForce rdf:type cacontology-taskforce:StateICACtaskForce ;
    rdfs:label "Utah Internet Crimes Against Children Task Force"@en ;
    rdfs:comment "State-level ICAC task force serving Utah, hosting Operation Hive Strike."@en ;
    cacontology-taskforce:coverageArea "Utah" ;
    cacontology-taskforce:taskForceType "state_level" ;
    cacontology-taskforce:operationalStatus "active" ;
    cacontology-taskforce:hostsOperation ex:OperationHiveStrike .

# Large-Scale Agency Coordination
ex:HiveStrikeCoordination rdf:type cacontology-multi:LargeScaleAgencyCoordination ,
                                   gufo:Situation ;
    rdfs:label "Operation Hive Strike Agency Coordination"@en ;
    rdfs:comment "Coordination of 31 federal, state, and local agencies with 80+ agents for Operation Hive Strike."@en ;
    cacontology-multi:agencyParticipantCount 31 ;
    cacontology-multi:agencyLevelsMixed true ;
    cacontology-multi:coordinatesAgencies ex:UtahAttorneyGeneralOffice, ex:UtahDepartmentOfPublicSafety, ex:FBI_SaltLakeCity, ex:HSI_SaltLake, ex:SaltLakeCityPD, ex:DavisCountySO ;
    cacontology-multi:deploysAgents ex:HiveStrikeAgentDeployment .

# Agent Deployment Coordination
ex:HiveStrikeAgentDeployment rdf:type cacontology-multi:AgentDeploymentCoordination ;
    rdfs:label "Operation Hive Strike Agent Deployment"@en ;
    rdfs:comment "Deployment of 80+ task force agents across 31 agencies for 4-day operation."@en ;
    cacontology-multi:agentParticipantCount 80 ;
    cacontology-multi:deploysAgents ex:UndercoverAgentRole, ex:DigitalForensicsRole, ex:SurveillanceRole, ex:ArrestTeamRole .

# Multi-Level Agency Participation
ex:HiveStrikeCoordination rdf:type cacontology-multi:MultiLevelAgencyParticipation ;
    cacontology-multi:agencyLevelsMixed true .

