# =============================================================================
# UNDERCOVER OPERATIONS COMPONENT
# =============================================================================

# Social Media Undercover Operation
ex:HiveStrikeSocialMediaOperation rdf:type cacontology-undercover:SocialMediaUndercoverOperation ;
    rdfs:label "Operation Hive Strike Social Media Component"@en ;
    rdfs:comment "Social media undercover component using various platforms to identify predators."@en ;
    cacontology-undercover:socialMediaPlatformsUsed 5 ;
    cacontology-undercover:conductedOnPlatform ex:SocialMediaPlatform1, ex:SocialMediaPlatform2, ex:SocialMediaPlatform3 ;
    cacontology-undercover:involvesChatInvestigation ex:HiveStrikeChatInvestigation .

# Multi-Platform Undercover Operation
ex:HiveStrikeSocialMediaOperation rdf:type cacontology-undercover:MultiPlatformUndercoverOperation ;
    cacontology-undercover:socialMediaPlatformsUsed 5 .

# Undercover Chat Investigation
ex:HiveStrikeChatInvestigation rdf:type cacontology-undercover:UndercoverChatInvestigation ;
    rdfs:label "Operation Hive Strike Chat Investigation"@en ;
    rdfs:comment "Chat investigations on various social media platforms with agents posing as minors."@en ;
    cacontology-undercover:chatPlatformType "social_media" ;
    cacontology-undercover:communicationMethod "text_chat" ;
    cacontology-undercover:chatDurationHours 96.0 ;
    cacontology-undercover:conversationCount 45 ;
    cacontology-undercover:leadsToSolicitation ex:HiveStrikeMeetingSolicitation .

# Minor Persona Operation
ex:HiveStrikeMinorPersonaOp rdf:type cacontology-undercover:MinorPersonaOperation ;
    rdfs:label "Operation Hive Strike Minor Persona Component"@en ;
    rdfs:comment "Agents posing as minors to identify individuals seeking inappropriate contact with children."@en ;
    cacontology-undercover:utilizesPersona ex:MinorPersonaAgent1, ex:MinorPersonaAgent2, ex:MinorPersonaAgent3 .

# In-Person Meeting Solicitation Detection
ex:HiveStrikeMeetingSolicitation rdf:type cacontology-undercover:InPersonMeetingSolicitation ;
    rdfs:label "Operation Hive Strike Meeting Solicitation Detection"@en ;
    rdfs:comment "Detection of individuals seeking to meet children in-person for sexual activity."@en ;
    cacontology-undercover:meetingSolicitationAttempts 23 ;
    cacontology-undercover:targetsIndividual ex:Suspect001, ex:Suspect002, ex:Suspect003 .

# Predator Targeting Operation
ex:HiveStrikePredatorTargeting rdf:type cacontology-undercover:PredatorTargetingOperation ;
    rdfs:label "Operation Hive Strike Predator Targeting"@en ;
    rdfs:comment "Targeting online predators seeking contact with children for illegal purposes."@en ;
    cacontology-undercover:targetBehaviorType "in_person_meeting_solicitation" ;
    cacontology-undercover:predatorContactAttempts 37 ;
    cacontology-undercover:identificationSuccessRate 0.68 ;
    cacontology-undercover:identifiesPredator ex:Suspect001, ex:Suspect002, ex:Suspect003, ex:Suspect004, ex:Suspect005 .

