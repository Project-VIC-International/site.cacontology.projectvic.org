@prefix : <http://example.org/vermont-case#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix uco-core: <https://ontology.unifiedcyberontology.org/uco/core/> .
@prefix uco-identity: <https://ontology.unifiedcyberontology.org/uco/identity/> .
@prefix uco-location: <https://ontology.unifiedcyberontology.org/uco/location/> .
@prefix uco-action: <https://ontology.unifiedcyberontology.org/uco/action/> .
@prefix cacontology: <https://cacontology.projectvic.org#> .
@prefix cacontology-production: <https://cacontology.projectvic.org/production#> .
@prefix cacontology-partnerships: <https://cacontology.projectvic.org/partnerships#> .
@prefix cacontology-international: <https://cacontology.projectvic.org/international#> .

# Example modeling of the Vermont case (Brian Bluto) using CAC ontology
# This demonstrates the Vermont case enhancements implemented in the ontology modules

# =============================================================================
# CASE OVERVIEW - Project Safe Childhood Case
# =============================================================================

:VermontCase2024 rdf:type cacontology-partnerships:ProjectSafeChildhoodCase ;
    rdfs:label "Brian Bluto Production Case"@en ;
    rdfs:comment "Vermont case involving sophisticated covert production and international distribution of CSAM"@en ;
    cacontology:caseNumber "VT-2024-HSI-001" ;
    cacontology:caseStatus "sentenced" ;
    cacontology:hasInvestigationMethod :InternationalCoordination ;
    cacontology:hasStep :VermontProduction ,
                 :InternationalAlert ,
                 :SearchAndSeizure .

# =============================================================================
# PROJECT SAFE CHILDHOOD PROGRAM IMPLEMENTATION
# =============================================================================

:ProjectSafeChildhoodProgram rdf:type cacontology-partnerships:NationalInitiativeProgram ;
    rdfs:label "Project Safe Childhood"@en ;
    rdfs:comment "Department of Justice initiative coordinating federal, state, and local efforts"@en ;
    cacontology-partnerships:initiativeName "Project_Safe_Childhood" ;
    cacontology-partnerships:launchedDate "2006-05-17T00:00:00"^^xsd:dateTime ;
    cacontology-partnerships:programScope "federal" ;
    cacontology-partnerships:leadAgency "DOJ" ;
    cacontology-partnerships:casesProcessed 15000 ;
    cacontology-partnerships:includesPartner :HSI , :VermontICAC , :USAOVermont .

:HSI rdf:type uco-identity:Organization ;
    rdfs:label "Homeland Security Investigations"@en .

:VermontICAC rdf:type uco-identity:Organization ;
    rdfs:label "Vermont ICAC Task Force"@en .

:USAOVermont rdf:type uco-identity:Organization ;
    rdfs:label "US Attorney's Office - Vermont District"@en .

# =============================================================================
# INTERNATIONAL COORDINATION (Australian Police â†’ HSI)
# =============================================================================

:InternationalAlert rdf:type cacontology-international:InternationalReferral ;
    rdfs:label "Australian Police to HSI Referral"@en ;
    rdfs:comment "Queensland Police Service undercover operation leading to Vermont suspect identification"@en ;
    cacontology-international:alertingPartner :QueenslandPolice ;
    cacontology-international:undercoverCoordination true ;
    cacontology-international:coordinationMethod "intelligence_sharing" ;
    cacontology-international:responseTimeInternational 24.0 ; # 24 hours
    uco-action:startTime "2024-01-15T10:00:00"^^xsd:dateTime ;
    uco-action:endTime "2024-01-16T10:00:00"^^xsd:dateTime .

:USAustraliaOperation rdf:type cacontology-international:USAustraliaCoordination ;
    rdfs:label "US-Australia Undercover Coordination"@en ;
    rdfs:comment "Coordination between Australian undercover operation and US follow-up investigation"@en ;
    cacontology-international:operationContext "undercover_investigation" ;
    cacontology-international:coordinatingAgency :HSI ;
    cacontology-international:coordinatesWith :QueenslandPolice .

:QueenslandPolice rdf:type uco-identity:Organization ;
    rdfs:label "Queensland Australia Police Service"@en ;
    rdfs:comment "Australian law enforcement conducting undercover operations"@en .

# =============================================================================
# SOPHISTICATED COVERT PRODUCTION IMPLEMENTATION
# =============================================================================

:VermontProduction rdf:type cacontology-production:ExtendedProductionPeriod ;
    rdfs:label "Brian Bluto Extended Production Period"@en ;
    rdfs:comment "3+ years of systematic covert surveillance and recording in private bathroom"@en ;
    cacontology-production:productionPeriod 1095 ; # ~3 years in days
    cacontology-production:victimAgeProgression "13-16" ;
    cacontology-production:systematicNature true ;
    cacontology-production:developmentalDocumentation true ;
    cacontology-production:productionStartDate "2021-01-01T00:00:00"^^xsd:dateTime ;
    cacontology-production:productionEndDate "2024-01-15T00:00:00"^^xsd:dateTime ;
    cacontology-production:sessionCount 365 ; # Approximately daily recording
    cacontology-production:imageCount 400 ;
    cacontology-production:videoCount 200 ;
    cacontology-production:victimCount 1 ;
    cacontology-production:distributionIntent "intended_distribution" ;
    cacontology-production:immediateDistribution true ;
    cacontology-production:usesEquipment :HiddenBackpackCamera ;
    cacontology-production:producedAt :BathroomLocation ;
    cacontology-production:involvestVictim :PrimaryVictim ;
    cacontology-production:producedBy :BrianBluto .

# =============================================================================
# DEVICE CONCEALMENT AND PRIVATE SPACE SURVEILLANCE
# =============================================================================

:HiddenBackpackCamera rdf:type cacontology-production:HiddenRecordingDevice ;
    rdfs:label "Concealed Camera in Backpack"@en ;
    rdfs:comment "Camera hidden in backpack with custom fabric modification for bathroom surveillance"@en ;
    cacontology-production:deviceBrand "Generic" ;
    cacontology-production:recordingCapability "both" ;
    cacontology-production:recordingQuality "medium" ;
    cacontology-production:storageCapacity 32.0 ; # 32GB storage
    cacontology-production:hasConcealment :BackpackConcealment .

:BackpackConcealment rdf:type cacontology-production:DeviceConcealment ;
    rdfs:label "Backpack Fabric Modification"@en ;
    rdfs:comment "Custom fabric modification to enable covert recording through backpack"@en ;
    cacontology-production:concealmentMethod "fabric_cut" ;
    cacontology-production:concealmentLocation "bathroom" ;
    cacontology-production:modificationDescription "Custom cut in backpack fabric allowing camera lens to protrude covertly" ;
    cacontology-production:usesContainer :ModifiedBackpack .

:ModifiedBackpack rdf:type cacontology-production:ConcealmentContainer ;
    rdfs:label "Modified Backpack Container"@en ;
    rdfs:comment "Backpack physically modified to conceal recording equipment"@en .

:PhysicalModification rdf:type cacontology-production:PhysicalDeviceModification ;
    rdfs:label "Backpack Fabric Cutting"@en ;
    rdfs:comment "Physical alteration of backpack fabric to enable covert camera placement"@en ;
    cacontology-production:modificationDescription "Precise cutting of backpack material to create hidden camera aperture" ;
    uco-action:performedBy :BrianBluto .

# =============================================================================
# PRIVATE SPACE SURVEILLANCE SPECIFICS
# =============================================================================

:BathroomSurveillanceAction rdf:type cacontology-production:BathroomSurveillance ;
    rdfs:label "Covert Bathroom Recording"@en ;
    rdfs:comment "Systematic surveillance of bathroom where victim had high expectation of privacy"@en ;
    cacontology-production:privacyExpectation "high" ;
    cacontology-production:surveillanceAngle "pointing_toward_bathroom" ;
    cacontology-production:victimAwareness "unaware" ;
    cacontology-production:sessionDuration 30 ; # 30 minutes average per session
    uco-action:performedBy :BrianBluto ;
    uco-action:location :BathroomLocation .

:BathroomLocation rdf:type cacontology-production:ResidentialProductionSite ;
    rdfs:label "Residence Bathroom"@en ;
    rdfs:comment "Private bathroom in Alburgh, Vermont residence"@en ;
    cacontology-production:locationSecurity "private" ;
    cacontology-production:locationAccess "controlled" ;
    uco-location:hasAddress :AlburghAddress .

:AlburghAddress rdf:type uco-location:Address ;
    rdfs:label "Alburgh, Vermont Residence"@en ;
    uco-location:city "Alburgh" ;
    uco-location:administrativeArea "Vermont" ;
    uco-location:country "United States" .

# =============================================================================
# PARTICIPANTS AND ROLES
# =============================================================================

:BrianBluto rdf:type cacontology-production:Producer ;
    rdfs:label "Brian Bluto"@en ;
    rdfs:comment "60-year-old defendant from Alburgh, Vermont"@en ;
    cacontology:hasAge 60 ;
    cacontology:hasSentence :SentenceDetails .

:PrimaryVictim rdf:type cacontology-production:ProductionVictim ;
    rdfs:label "Primary Victim"@en ;
    rdfs:comment "Victim recorded during ages 13-16 over 3+ year period"@en ;
    cacontology-production:victimAge 13 ; # Starting age
    cacontology:hasVictimStatus "identified" .

:SentenceDetails rdf:type cacontology:Sentence ;
    rdfs:label "Federal Sentence"@en ;
    rdfs:comment "144 months imprisonment plus 15 years supervised release"@en ;
    cacontology:imprisonmentMonths 144 ;
    cacontology:supervisedReleaseYears 15 ;
    cacontology:hasCharges :ProductionCharges .

:ProductionCharges rdf:type cacontology:CriminalCharges ;
    rdfs:label "Production and Distribution Charges"@en ;
    rdfs:comment "Federal charges for production and distribution of child sexual abuse material"@en ;
    cacontology:chargeType "production_and_distribution" ;
    cacontology:federalCharges true .

# =============================================================================
# SEARCH AND SEIZURE OPERATION
# =============================================================================

:SearchAndSeizure rdf:type uco-action:Action ;
    rdfs:label "Search Warrant Execution"@en ;
    rdfs:comment "HSI execution of search warrant resulting in device seizure and evidence recovery"@en ;
    uco-action:performedBy :HSI ;
    uco-action:location :AlburghAddress ;
    cacontology-production:evidenceRecovered true ;
    cacontology-production:metadataPreserved true ;
    cacontology-production:forensicValue "high" .

# =============================================================================
# CONTENT AND DISTRIBUTION
# =============================================================================

:ProducedMaterial rdf:type cacontology-production:ProducedVideo ;
    rdfs:label "Covertly Recorded Material"@en ;
    rdfs:comment "Video material produced through systematic bathroom surveillance"@en ;
    cacontology-production:recordedWith :HiddenBackpackCamera ;
    cacontology-production:metadataPreserved true ;
    cacontology-production:forensicValue "high" ;
    cacontology-production:evidenceOf :VermontProduction .

# =============================================================================
# RELATIONSHIPS AND CONNECTIONS
# =============================================================================

# Link case to Project Safe Childhood program
:VermontCase2024 cacontology-partnerships:partOfProgram :ProjectSafeChildhoodProgram .

# Link international coordination to case
:VermontCase2024 cacontology:hasInternationalElement :InternationalAlert .

# Link production activities
:VermontProduction cacontology-production:partOfSeries :VermontCase2024 .
:BathroomSurveillanceAction cacontology-production:partOfSeries :VermontProduction .

# Equipment relationships
:VermontProduction cacontology-production:usesEquipment :HiddenBackpackCamera .
:ProducedMaterial cacontology-production:recordedWith :HiddenBackpackCamera .

# Evidence relationships
:ProducedMaterial cacontology-production:evidenceOf :VermontProduction .
:SearchAndSeizure cacontology:recoversEvidence :ProducedMaterial . 