    cacontology-sextortion:actualAge 20 ;
    cacontology-sextortion:claimedAge 16 ;
    cacontology-sextortion:falsePersonaType "peer" .

:False_Profile rdf:type cacontology-sextortion:FalseProfileCreation ;
    rdfs:label "False Profile Creation"@en ;
    uco-core:description "Created false profile on instant messaging platform with fake age information" .

:Trust_Building rdf:type cacontology-sextortion:TrustBuildingPhase ;
    rdfs:label "Trust Building Phase"@en ;
    uco-core:description "Building rapport and trust with victims through false peer identity" ;
    cacontology-sextortion:progressionStage 2 .

:Sexual_Solicitation rdf:type cacontology-sextortion:SexualSolicitationPhase ;
    rdfs:label "Sexual Solicitation Phase"@en ;
